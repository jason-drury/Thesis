  journal = {Monthly Notices of the Royal Astronomical Society},
  doi = {10.1093/mnras/stx1056},
  author = {Colman, Isabel L. and Huber, Daniel and Bedding, Timothy R. and Kuszlewicz, James S. and Yu, Jie and Beck, Paul G. and Elsworth, Yvonne and Garc{\'i}a, Rafael A. and Kawaler, Steven D. and Mathur, Savita and Stello, Dennis and White, Timothy R.},
