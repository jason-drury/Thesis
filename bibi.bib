  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Lagarde et al_2017_Population synthesis to constrain Galactic and stellar physics - I.pdf;/home/jdru4706/Zotero/storage/YGF96MQS/aa30253-16.html},
  journal = {A\&A},
  language = {en}
}

@article{lagarde_testing_2016,
