@article{netopil_metallicity_2016,
  title = {On the Metallicity of Open Clusters},
  author = {Netopil, M and Paunzen, E and Heiter, U and Soubiran, C},
  year = {2016},
  pages = {17},
  abstract = {Methods. We used photometric and spectroscopic data to derive cluster metallicities. The different sources were checked and tested for possible offsets and correlations. Results. In total, metallicities for 172 open cluster were derived. We used the spectroscopic data of 100 objects for a study of the radial metallicity distribution and the age-metallicity relation. We found a possible increase of metallicity with age, which, if confirmed, would provide observational evidence for radial migration. Although a statistical significance is given, more studies are certainly needed to exclude selection effects, for example. The comparison of open clusters and Cepheids with recent Galactic models agrees well in general. However, the models do not reproduce the flat gradient of the open clusters in the outer disc. Thus, the effect of radial migration is either underestimated in the models, or an additional mechanism is at work. Conclusions. Apart from the Cepheids, open clusters are the best tracers for metallicity over large Galactocentric distances in the Milky Way. For a sound statistical analysis, a sufficiently large and homogeneous sample of cluster metallicities is needed. Our compilation is currently by far the largest and provides the basis for several basic studies such as the statistical treatment of the Galactic cluster population, or evolutionary studies of individual star groups in open clusters.},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Netopil et al_2016_On the metallicity of open clusters.pdf},
  language = {en}
}

