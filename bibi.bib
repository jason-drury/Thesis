}

@article{montet_young_2020,
  title = {The {{Young Planet DS Tuc Ab}} Has a {{Low Obliquity}}},
  author = {Montet, Benjamin T. and Feinstein, Adina D. and Luger, Rodrigo and Bedell, Megan E. and {Gully-Santiago}, Michael A. and Teske, Johanna K. and Wang, Sharon Xuesong and Butler, R. Paul and Flowers, Erin and Shectman, Stephen A. and Crane, Jeffrey D. and Thompson, Ian B.},
  year = {2020},
  month = feb,
  volume = {159},
