  abstract = {The Open Cluster Chemical Analysis and Mapping (OCCAM) survey aims to produce a comprehensive, uniform, infrared-based data set for hundreds of open clusters, and constrain key Galactic dynamical and chemical parameters from this sample. This first contribution from the OCCAM survey presents analysis of 141 members stars in 28 open clusters with high-resolution metallicities derived from a large uniform sample collected as part of the Sloan Digital Sky Survey III/Apache Point Observatory Galactic Evolution Experiment. This sample includes the first high-resolution metallicity measurements for 22 open clusters. With this largest ever uniformly observed sample of open cluster stars we investigate the Galactic disk gradients of both [M/H] and [{$\alpha$}/M]. We find basically no gradient in [{$\alpha$}/M] across 7.9 kpc {$\leqslant$} R GC {$\leqslant$} 14.5 kpc, but [M/H] does show a gradient for R GC {$<$} 10 kpc and a significant flattening beyond R GC = 10 kpc. In particular, whereas fitting a single linear trend yields an [M/H] gradient of \textendash 0.09 {$\pm$} 0.03 dex kpc\textendash 1\textemdash similar to previously measure gradients inside 13 kpc\textemdash by independently fitting inside and outside 10 kpc separately we find a significantly steeper gradient near the Sun (7.9 {$\leqslant$} R GC {$\leqslant$} 10) than previously found (\textendash 0.20 {$\pm$} 0.08 dex kpc\textendash 1) and a nearly flat trend beyond 10 kpc (\textendash 0.02 {$\pm$} 0.09 dex kpc\textendash 1).},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Frinchaboy et al_2013_THE OPEN CLUSTER CHEMICAL ANALYSIS AND MAPPING SURVEY.pdf;/home/jdru4706/Dropbox_USyd/Apps/Zotero/Frinchaboy et al_2013_THE OPEN CLUSTER CHEMICAL ANALYSIS AND MAPPING SURVEY2.pdf},
  journal = {ApJL},
  language = {en},
  number = {1}
}

@article{frolov_proper_2010,
  title = {Proper Motions and {{CCD}} Photometry of Stars in the Region of the Open Cluster {{NGC}} 6866},
  author = {Frolov, V. N. and Ananjevskaja, Yu. K. and Gorshanov, D. L. and Polyakov, E. V.},
  year = {2010},
  month = may,
  volume = {36},
  pages = {338--348},
  issn = {1562-6873},
  doi = {10.1134/S106377371005004X},
  abstract = {We present the results of our comprehensive study of the Galactic open star cluster NGC 6866. The positions of stars in the investigated region have been obtained with the ``Fantasy'' automatic measuring machine from 10 plates of the normal astrograph at the Pulkovo Astronomical Observatory. The size of the investigated field is 40{${'}$} \texttimes{} 40{${'}$}, the limiting magnitude is B {$\sim$} 16 {$\cdot$} m 6, and the maximum epoch difference is 79 yr. For 1202 field stars, we have determined the relative proper motions with an rms error of 2.5 mas yr-1. Out of them, 423 stars may be considered cluster members with a probability P {$>$} 70\% according to the astrometric criterion. Photometric diagrams have been used as an additional criterion. We have performed two-color BV CCD photometry of stars with the Pulkovo ZA-320M mirror astrograph. The U magnitudes from the literature have also been used to construct the two-color diagrams. A total of 267 stars have turned out to be members of NGC 6866 according to the two criteria. We present refined physical parameters of the cluster and its age estimate (5.6 \texttimes{} 108 yr). The cluster membership of red and blue giants, variable, double, and multiple stars is considered. We have found an almost complete coincidence of the positions of one of the stars in the region (a cluster nonmember) and a soft X-ray source in the ROSAT catalog. The ``Fantasy'' automatic measuring machine is described in the Appendix.},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Frolov et al_2010_Proper motions and CCD photometry of stars in the region of the open cluster.pdf},
  journal = {Astron. Lett.},
  keywords = {open star clusters: NGC 6866,photometry,proper motions},
  language = {en},
  number = {5}
}

@article{fujii_survival_2018,
  title = {Survival {{Rates}} of {{Planets}} in {{Open Clusters}}: The {{Pleiades}}, {{Hyades}}, and {{Praesepe}} Clusters},
  shorttitle = {Survival {{Rates}} of {{Planets}} in {{Open Clusters}}},
  author = {Fujii, M. S. and Hori, Y.},
  year = {2018},
  month = nov,
  abstract = {In clustered environments, stellar encounters can liberate planets from their host stars via close encounters. Although the detection probability of planets suggests that the planet population in open clusters resembles that in the field, only dozens of planet-hosting stars have been discovered in open clusters. We explore the survival rates of planets against stellar encounters in open clusters similar to the Pleiades, Hyades, and Praesepe and embedded clusters. We perform a series of N-body simulations of high-density and low-density open clusters, open clusters that grow via mergers of subclusters, and embedded clusters. We semi-analytically calculate the survival rate of planets in star clusters up to \textasciitilde 1 Gyr using relative velocities, masses, and impact parameters of intruding stars. Less than 1.5\% of close-in planets within 1 AU and at most 7\% of planets with 1--10 AU are ejected by stellar encounters in clustered environments after the dynamical evolution of star clusters. If a planet population from 0.01--100 AU in an open cluster initially follows the probability distribution function of exoplanets discovered in the field by RV surveys with semi-major axis (\$a\_\{\textbackslash rm p\}\$) between 0.03--3 AU (\$\textbackslash propto a\_\{\textbackslash rm p\}\^\{-0.6\}\$), the PDF of surviving planets beyond \textasciitilde 10 AU in open clusters follows \$\textbackslash propto a\_\{\textbackslash rm p\}\^\{-0.76\}\$, which is consistent with the result of direct-imaging surveys. The production rate of free-floating planets (FFPs) per star is 0.0096--0.18, where we assume that all the stars initially have one giant planet with a mass of 1--13 \$M\_\{\textbackslash rm Jup\}\$ in a circular orbit. The expected frequency of FFPs is compatible with the upper limit on that of FFPs indicated by recent microlensing surveys.},
  archivePrefix = {arXiv},
  eprint = {1811.08598},
  eprinttype = {arxiv},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Fujii_Hori_2018_Survival Rates of Planets in Open Clusters.pdf;/home/jdru4706/Zotero/storage/DCT7EYJE/1811.html},
  journal = {arXiv:1811.08598 [astro-ph]},
  keywords = {Astrophysics - Earth and Planetary Astrophysics},
  note = {Comment: 19 pages, 12 figures, submitted to A\&A},
  primaryClass = {astro-ph}
}

