  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/6G82JPFY/Kalirai et al. - 2001 - The CFHT Open Star Cluster Survey. I. Cluster Sele.pdf}
}

@article{yang_wiyn_2013-1,
  title = {{{WIYN Open Cluster Study LII}}: {{Wide}}-Field {{CCD Photometry}} of the {{Old Open Cluster NGC}} 6819},
  volume = {762},
