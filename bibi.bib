  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Ruiz-Lapuente et al_2018_Tycho's supernova.pdf;/home/jdru4706/Zotero/storage/97YG9ZG3/1807.html},
  journal = {arXiv:1807.03593 [astro-ph]},
  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Solar and Stellar Astrophysics},
  note = {Comment: Accepted by ApJ, emulated ApJ version, 5 tables, 6 figures},
  primaryClass = {astro-ph}
}

