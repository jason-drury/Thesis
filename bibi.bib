  author = {Grunblatt, Samuel K. and Huber, Daniel and Gaidos, Eric and Lopez, Eric D. and Howard, Andrew W. and Isaacson, Howard T. and Sinukoff, Evan and Vanderburg, Andrew and Nofi, Larissa and Yu, Jie and North, Thomas S. H. and Chaplin, William and {Foreman-Mackey}, Daniel and Petigura, Erik and Ansdell, Megan and Weiss, Lauren and Fulton, Benjamin and Lin, Douglas N. C.},
