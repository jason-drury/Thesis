  author = {Carrera, R. and Bragaglia, A. and {Cantat-Gaudin}, T. and Vallenari, A. and {Balaguer-N{\'u}{\~n}ez}, L. and Bossini, D. and Casamiquela, L. and Jordi, C. and Sordo, R. and Soubiran, C.},
