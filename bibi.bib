  author = {Wu, Tao and Deng, Zhen-Ming and Li, Yan},
  pages = {6},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Wu et al. - ROTATIONAL SPLITS OF OSCILLATION MODES AND MODE ID.pdf}
}

