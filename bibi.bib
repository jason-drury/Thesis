  title = {Asteroseismic Masses, Ages, and Core Properties of \$\textbackslash{}gamma\$ {{Doradus}} Stars Using Gravito-Inertial Dipole Modes and Spectroscopy},
  abstract = {The asteroseismic modelling of period spacing patterns from gravito-inertial modes in stars with a convective core is a high-dimensional problem. We utilise the measured period spacing pattern of prograde dipole gravity modes (acquiring \$\textbackslash{}Pi\_0\$), in combination with the effective temperature (\$T\_\{\textbackslash{}rm eff\}\$) and surface gravity (\$\textbackslash{}log g\$) derived from spectroscopy, to estimate the fundamental stellar parameters and core properties of 37 \$\textbackslash{}gamma\textasciitilde\$Doradus (\$\textbackslash{}gamma\textasciitilde\$Dor) stars whose rotation frequency has been derived from \$\textbackslash{}textit\{Kepler\}\$ photometry. We make use of two 6D grids of stellar models, one with step core overshooting and one with exponential core overshooting, to evaluate correlations between the three observables \$\textbackslash{}Pi\_0\$, \$T\_\{\textbackslash{}rm eff\}\$, and \$\textbackslash{}log g\$ and the mass, age, core overshooting, metallicity, initial hydrogen mass fraction and envelope mixing. We provide multivariate linear model recipes relating the stellar parameters to be estimated to the three observables (\$\textbackslash{}Pi\_0\$, \$T\_\{\textbackslash{}rm eff\}\$, \$\textbackslash{}log g\$). We estimate the (core) mass, age, core overshooting and metallicity of \$\textbackslash{}gamma\textasciitilde\$Dor stars from an ensemble analysis and achieve relative uncertainties of \$\textbackslash{}sim\textbackslash{}!10\$ per cent for the parameters. The asteroseismic age determination allows us to conclude that efficient angular momentum transport occurs already early on during the main sequence. We find that the nine stars with observed Rossby modes occur across almost the entire main-sequence phase, except close to core-hydrogen exhaustion. Future improvements of our work will come from the inclusion of more types of detected modes per star, larger samples, and modelling of individual mode frequencies.},
  journal = {arXiv:1902.06746 [astro-ph]},
  author = {Mombarg, Joey S. G. and Van Reeth, Timothy and Pedersen, May G. and Molenberghs, Geert and Bowman, Dominic M. and Johnston, Cole and Tkachenko, Andrew and Aerts, Conny},
  month = feb,
  year = {2019},
  keywords = {Astrophysics - Solar and Stellar Astrophysics},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/3GR8RGWA/Mombarg et al. - 2019 - Asteroseismic masses, ages, and core properties of.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/HAFV3KJU/1902.html},
  annote = {Comment: Accepted for publication in MNRAS}
}

@article{karoff_sounding_2019,
  archivePrefix = {arXiv},
  eprinttype = {arxiv},
  eprint = {1902.02172},
  primaryClass = {astro-ph},
  title = {Sounding Stellar Cycles with {{Kepler}} - {{III}}. {{Comparative}} Analysis of Chromospheric, Photometric and Asteroseismic Variability},
  abstract = {By combining ground-based spectrographic observations of variability in the chromospheric emission from Sun-like stars with the variability seen in their eigenmode frequencies, it is possible to relate the changes observed at the surfaces of these stars to the changes taking place in the interior. By further comparing this variability to changes in the relative flux from the stars, one can obtain an expression for how these activity indicators relate to the energy output from the stars. Such studies become very pertinent when the variability can be related to stellar cycles as they can then be used on improve our understanding of the solar cycle and its effect on the energy output from the Sun. Here we present observations of chromospheric emission in 20 Sun-like stars obtained over the course of the nominal 4-year Kepler mission. Even though 4 years is not short to detect stellar cycles, observations from the Kepler mission can still be used to analyse the variability of the different activity indicators thereby obtaining information of the physical mechanism generating the variability. The analysis reveals no strong correlation between the different activity indicators, except in very few cases. We suggest that this is due to the sparse sampling of our ground-based observations on the one hand and that we are likely not tracing cyclic variability on the other hand. We also discuss how to improve the situation.},
  journal = {arXiv:1902.02172 [astro-ph]},
  author = {Karoff, C. and Metcalfe, T. S. and Montet, B. T. and Jannsen, N. E. and Santos, A. R. G. and Nielsen, M. B. and Chaplin, W. J.},
  month = feb,
  year = {2019},
  keywords = {Astrophysics - Solar and Stellar Astrophysics},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/HPYFAJI5/Karoff et al. - 2019 - Sounding stellar cycles with Kepler - III. Compara.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/TWFB9AEV/1902.html},
  annote = {Comment: submitted to MNRAS}
}

@article{compton_asteroseismology_2019,
  archivePrefix = {arXiv},
  eprinttype = {arxiv},
  eprint = {1902.05561},
  title = {Asteroseismology of Main-Sequence {{F}} Stars with \textbackslash{}textit\{\vphantom\}{{Kepler}}\vphantom\{\}: Overcoming Short Mode Lifetimes},
  volume = {485},
  issn = {0035-8711, 1365-2966},
  shorttitle = {Asteroseismology of Main-Sequence {{F}} Stars with \textbackslash{}textit\{\vphantom\}{{Kepler}}\vphantom\{\}},
  abstract = {Asteroseismology is a powerful way of determining stellar parameters and properties of stars like the Sun. However, main-sequence F-type stars exhibit short mode lifetimes relative to their oscillation frequency, resulting in overlapping radial and quadrupole modes. The goal of this paper is to use the blended modes for asteroseismology in place of the individual separable modes. We used a peak-bagging method to measure the centroids of radial-quadrupole pairs for 66 stars from the \textbackslash{}textit\{Kepler\} LEGACY sample, as well as \{\textbackslash{}theta\} Cyg, HD 49933, HD 181420, and Procyon. We used the relative quadrupole-mode visibility to estimate a theoretical centroid frequency from a grid of stellar oscillation models. The observed centroids were matched to the modelled centroids with empirical surface correction to calculate stellar parameters. We find that the stellar parameters returned using this approach agree with the results using individual mode frequencies for stars, where those are available. We conclude that the unresolved centroid frequencies can be used to perform asteroseismology with an accuracy similar to that based on individual mode frequencies.},
  number = {1},
  journal = {Monthly Notices of the Royal Astronomical Society},
  doi = {10.1093/mnras/stz432},
  author = {Compton, Douglas L. and Bedding, Timothy R. and Stello, Dennis},
  month = may,
  year = {2019},
  keywords = {Astrophysics - Solar and Stellar Astrophysics},
  pages = {560-569},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/NL3LED4L/Compton et al. - 2019 - Asteroseismology of main-sequence F stars with te.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/K3H3R3YU/1902.html},
  annote = {Comment: 11 pages, 11 Figures, Published in MNRAS}
}

