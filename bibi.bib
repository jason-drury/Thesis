  doi = {10.3847/0004-637X/819/1/33},
  author = {Lehmann, H. and Borkovits, T. and Rappaport, S. A. and Ngo, H. and Mawet, D. and Csizmadia, Sz. and {Forg{\'a}cs-Dajka}, E.},
  month = feb,
