@phdthesis{leiner_beyond_2018,
  title = {Beyond {{Blue Stragglers}}: {{Binary Stellar Evolution Across}} the {{M67 Color}}-{{Magnitude Diagram}}},
  shorttitle = {Beyond {{Blue Stragglers}}},
  author = {Leiner, Emily M.},
  year = {2018},
  address = {{United States -- Wisconsin}},
  abstract = {Color-magnitude diagrams (CMDs) of open clusters reveal many stars that do not fall on standard isochrones, and thus are not evolving according to standard theory. The most numerous of these are the blue straggler stars (BSSs), stars brighter and bluer than the cluster turnoff that are likely formed from binary mass transfer, stellar mergers, or collisions during dynamical encounters. Many other non-standard stars are also found in fclusters, but their origins are not well understood. These include the `yellow stragglers' found between the BSS region and the red giant branch, and `sub-subgiants' found fainter and redder than the giant branch. Using the 4 Gyr open cluster M67 as an exemplar, I present results of the first comprehensive study to move beyond the BSS population and begin to understand these other types of non-standard stars. First, I present an asteroseismic analysis of a yellow straggler in M67, demonstrating that it is an evolved blue straggler star of twice the cluster turnoff mass. Next, I present theoretical formation models for the sub-subgiant stars. I focus on three hypotheses of sub-subgiant formation: Roche lobe overflow in binary systems, rapid envelope mass loss from subgiant stars, e.g. during grazing dynamical encounters, and inhibition of convection in giant stars with strong magnetic fields. Finally, I use K2 light curves to identify stars on the M67 main sequence with unusually fast rotation rates given the old age of the cluster. I hypothesize that these stars have been spun up due to recent mergers, collisions, or episodes of mass-transfer, and thus represent lower-luminosity analogs to the blue stragglers. Together, these studies present a much more complete view of the post-merger, collision, and mass-transfer population of M67, and demonstrate that BSSs account for only about half of the post-interaction population. These stars represent the early stages of binary evolution pathways that may lead to many astrophysically important objects including X-ray binaries, double degenerates, and Type 1a supernovae. Studying these products of binary evolution across cluster CMDs is a critical step towards developing better models of binary evolutionary pathways and understanding the impact of binaries on stellar populations.},
  copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Leiner_2018_Beyond Blue Stragglers.pdf},
  keywords = {Asteroseismology,Binary stars,M67,Mass transfer,Open clusters,Pure sciences,Stellar evolution},
  language = {English},
  school = {The University of Wisconsin - Madison},
  type = {Ph.{{D}}.}
}

@article{leiner_blue_2019,
  title = {Blue {{Lurkers}}: {{Hidden Blue Stragglers}} on the {{M67 Main Sequence Identified}} from Their {{Kepler}}/{{K2 Rotation Periods}}},
  shorttitle = {Blue {{Lurkers}}},
  author = {Leiner, Emily and Mathieu, Robert D. and Vanderburg, Andrew and Gosnell, Natalie M. and Smith, Jeffrey C.},
  year = {2019},
  month = apr,
  abstract = {At an age of 4 Gyr, typical solar-type stars in M67 have rotation rates of 20-30 days. Using K2 Campaign 5 and 16 light curves and the spectral archive of the WIYN Open Cluster Study, we identify eleven three-dimensional kinematic members of M67 with anomalously fast rotation periods of 2-8 days, implying ages of less than 1 Gyr. We hypothesize that these anomalously fast rotators have been spun up by mass transfer, mergers, or stellar collisions during dynamical encounters within the last Gyr, and thus represent lower-luminosity counterparts to the blue straggler stars. These 11 candidate post-interaction stellar systems have much in common with the blue stragglers including a high binary fraction (73\%), a number of long-period, low-eccentricity binary systems, and in at least one case a UV excess consistent with the presence of a hot white dwarf companion. The identification of these 11 systems provides the first picture of the low-luminosity end of the blue straggler distribution, providing new constraints for detailed binary evolution models and cluster population studies. This result also clearly demonstrates the need to properly account for the impact of binaries on stellar evolution, as significant numbers of post-interaction binaries likely exist on cluster main sequences and in the field. These stars are not always easy to identify, but make up \textasciitilde 10\% of the spectroscopic binary population among the solar-type stars in M67.},
  archivePrefix = {arXiv},
  eprint = {1904.02169},
  eprinttype = {arxiv},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Leiner et al_2019_Blue Lurkers.pdf;/home/jdru4706/Zotero/storage/B3VW2MXN/1904.html},
  journal = {arXiv:1904.02169 [astro-ph]},
  keywords = {Astrophysics - Solar and Stellar Astrophysics},
  note = {Comment: 24 pages, 7 figures, submitted to ApJ},
  primaryClass = {astro-ph}
}

@article{leiner_k2_2016,
  title = {The {{K2 M67 Study}}: {{An Evolved Blue Straggler}} in {{M67}} from {{K2 Mission Asteroseismology}}},
  shorttitle = {The {{K2 M67 Study}}},
  author = {Leiner, Emily and Mathieu, Robert D. and Stello, Dennis and Vanderburg, Andrew and Sandquist, Eric},
  year = {2016},
  volume = {832},
  pages = {L13},
  issn = {2041-8205},
  doi = {10.3847/2041-8205/832/1/L13},
  abstract = {Yellow straggler stars (YSSs) fall above the subgiant branch in optical color\textendash magnitude diagrams (CMDs), between the blue stragglers and the red giants. YSSs may represent a population of evolved blue stragglers, but none have the direct and precise mass and radius measurements needed to determine their evolutionary states and formation histories. Here we report the first asteroseismic mass and radius measurements of such a star, the yellow straggler S1237 in the open cluster M67. We apply asteroseismic scaling relations to a frequency analysis of the Kepler K2 light curve and find a mass of2.9 {$\pm$} 0.2 M {$\odot$} and a radius of9.2 {$\pm$} 0.2 R {$\odot$} . This is more than twice the mass of the main-sequence turnoff in M67, suggesting that S1237 is indeed an evolved blue straggler. S1237 is the primary in a spectroscopic binary. We update the binary orbital solution and use spectral energy distribution fitting to constrain the CMD location of the secondary star. We find that the secondary is likely an upper main-sequence star near the turnoff, but a slightly hotter blue straggler companion is also possible. We then compare the asteroseismic mass of the primary to its mass from CMD fitting, finding that the photometry implies a mass and radius more than 2 {$\sigma$} below the asteroseismic measurement. Finally, we consider formation mechanisms for this star and suggest that S1237 may have formed from dynamical encounters resulting in stellar collisions or a binary merger.},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Leiner et al_2016_The K2 M67 Study.pdf},
  journal = {ApJL},
  language = {en},
  number = {1}
}

@article{leiner_origin_2017,
  title = {On the {{Origin}} of {{Sub}}-Subgiant {{Stars}}. {{II}}. {{Binary Mass Transfer}}, {{Envelope Stripping}}, and {{Magnetic Activity}}},
  author = {Leiner, Emily and Mathieu, Robert D. and Geller, Aaron M.},
  year = {2017},
  month = may,
  volume = {840},
  pages = {67},
  issn = {1538-4357},
  doi = {10.3847/1538-4357/aa6aff},
  abstract = {Sub-subgiant stars (SSGs) lie to the red of the main sequence and fainter than the red giant branch in cluster color\textendash magnitude diagrams (CMDs), a region not easily populated by standard stellar evolution pathways. While there has been speculation on what mechanisms may create these unusual stars, no well-developed theory exists to explain their origins. Here we discuss three hypotheses of SSG formation: (1) mass transfer in a binary system, (2) stripping of a subgiant's envelope, perhaps during a dynamical encounter, and (3) reduced luminosity due to magnetic fields that lower convective efficiency and produce large starspots. Using the stellar evolution code MESA, we develop evolutionary tracks for each of these hypotheses, and compare the expected stellar and orbital properties of these models with six known SSGs in the two open clusters M67 and NGC 6791. All three of these mechanisms can create stars or binary systems in the SSG CMD domain. We also calculate the frequency with which each of these mechanisms may create SSG systems, and find that the magnetic field hypothesis is expected to create SSGs with the highest frequency in open clusters. Mass transfer and envelope stripping have lower expected formation frequencies, but may nevertheless create occasional SSGs in open clusters. They may also be important mechanisms to create SSGs in higher mass globular clusters.},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Leiner et al_2017_On the Origin of Sub-subgiant Stars.pdf},
  journal = {The Astrophysical Journal},
  language = {en},
  number = {2}
}

@misc{leiner_origins_2016,
  title = {On the {{Origins}} of {{Sub}}-{{Subgiants}}: {{Mass Transfer}}, {{Dynamical Encounters}}, and {{Magnetic Fields}}},
  shorttitle = {On the {{Origins}} of {{Sub}}-{{Subgiants}}},
  author = {Leiner, Emily and Geller, Aaron and Mathieu, Robert},
  year = {2016},
  month = jul,
  doi = {10.5281/zenodo.59151},
  abstract = {Sub-subgiant stars (SSGs) lie red of the main-sequence and fainter than the red giant branch in optical color-magnitude diagrams, a region not easily populated by standard stellar evolution pathways. Discoveries of dozens of SSGs in both open and globular clusters solidifies these stars as a unique class sharing several properties: X-ray emission, photometric variability, and, where binary status is known, orbital periods between 2-18 days.~While there has been speculation on what mechanisms may create these unusual stars, no theory has yet been developed to explain their origins. Here we focus on three hypotheses of SSG formation: (1) mass transfer in a binary system, (2) tidally induced mass loss from a subgiant star during a dynamical encounter, and (3) reduced luminosity due to magnetic fields that lower convective efficiency and produce large star spots.~Using the stellar evolution code MESA, we develop evolutionary tracks for each of these hypotheses, and compare the expected stellar and orbital properties of these models with the six known SSGs in the two open clusters M67 and NGC 6791. We calculate the frequency with which each of these mechanisms may create SSG systems, and find that the magnetic field hypothesis is expected to create SSGs with the highest frequency in open clusters. Mass transfer and dynamical encounters may also create SSGs, but have lower expected formation frequencies. They may be more important mechanisms in globular clusters with higher encounter rates and larger populations of subgiants.},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Leiner et al_2016_On the Origins of Sub-Subgiants.pdf},
  keywords = {binary stars,magnetic fields,open clusters}
}

