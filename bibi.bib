  author = {Chaplin, W. J. and Basu, S. and Huber, D. and Serenelli, A. and Casagrande, L. and Silva Aguirre, V. and Ball, W. H. and Creevey, O. L. and Gizon, L. and Handberg, R. and Karoff, C. and Lutz, R. and Marques, J. P. and Miglio, A. and Stello, D. and Suran, M. D. and Pricopi, D. and Metcalfe, T. S. and Monteiro, M. J. P. F. G. and {Molenda-{\.Z}akowicz}, J. and Appourchaux, T. and {Christensen-Dalsgaard}, J. and Elsworth, Y. and Garc{\'i}a, R. A. and Houdek, G. and Kjeldsen, H. and Bonanno, A. and Campante, T. L. and Corsaro, E. and Gaulme, P. and Hekker, S. and Mathur, S. and Mosser, B. and R{\'e}gulo, C. and Salabert, D.},
