  year = {2015},
  keywords = {Conference Poster},
  pages = {06061},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/WHAIXTDK/Sowicka et al. - 2015 - A search for pulsating blue stars in the vicinity .pdf}
}

@article{bellamy_new_2015,
  title = {New {{Seismic Members}} in {{NGC}} 6791 and {{NGC}} 6819},
  volume = {101},
  issn = {2100-014X},
  abstract = {We present preliminary results of an asteroseismic analysis of the open clusters NGC 6791 and NGC 6819 in the Kepler field of view. We analysed over 400 stars to extract the asteroseismic properties {$\nu$}max and {$\Delta\nu$} to make them available to the scientific community. We developed an interactive stitching algorithm that is used to correct for the instrumental drifts in the Kepler light curves and stitch the jumps between each quarter together. We use the SYD pipeline to extract the asteroseismic values and compare these to the expected values for cluster membership to find new member stars for each cluster. We compare our results to previous membership studies.},
  language = {en},
  journal = {EPJ Web of Conferences},
  doi = {10.1051/epjconf/201510106007},
  author = {Bellamy, B. and Stello, D.},
