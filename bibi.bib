  journal = {The Astronomical Journal},
  keywords = {Astronomical Observatories,Astronomical Photometry,Astronomical Spectroscopy,Cassegrain Optics,Charge Coupled Devices,Color-Magnitude Diagram,Gratings (Spectra),Images,Interstellar Extinction,Metal Clusters,Metallicity,NGC 6791,Optical Filters,Point Spread Functions,Radial Distribution,Red Giant Stars,Spectrographs,Telescopes,Velocity Distribution}
