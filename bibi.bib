@article{tian_asteroseismic_nodate,
  title = {Asteroseismic Analysis of Solar-like Star {{KIC}} 6225718 Observed by {{Kepler}}},
  abstract = {Context. Asteroseismology is a powerful tool for investigating physical processes in stars. Solar-like oscillations of main-sequence stars and red giant stars have been observed through ground-based observations and space missions. Aims. We analyzed seven seasons of short-cadence data, which was obtained with the Kepler Mission and corrected by WG\#1, of a solar-type star of spectral type F: KIC 6225718 and obtained more information concerning solar-like stars. Methods. We extracted oscillation modes from the power spectrum of KIC 6225718 and compared the theoretical acoustic spectrum with the extracted modes in order to determine the parameters of the star accurately. Results. We identified 33 p-modes with angular degree l=0-2 in the frequency range of 1600 - 2800 {$\mathrm{\mu}$}Hz, and obtained the large frequency separation {$\increment\nu$} = 105.78 {$\pm$} 0.65 {$\mathrm{\mu}$}Hz and the frequency of maximum power spectrum {$\nu$}max = 2301 {$\pm$} 21 {$\mathrm{\mu}$}Hz. We got values for the star's physical parameters: the range of mass M = 1.13{$\pm$}0.03M{$\odot$}, the radius R = 1.23{$\pm$}0.01R{$\odot$}, and the age t = 3.03{$\pm$}0.42 Gyr, which is at the phase of main sequence. We analyzed the effects of overshooting, and determined that the models with overshooting {$\alpha$}ov = 0.2 are in better agreement with observations. The ratio of small to large separations r01 which is related to the internal structure of the star could be used to test physical processes in stellar interiors free from the near-surface effects.},
  language = {en},
  author = {Tian, Zhijia and Bi, Shaolan and Yang, Wuming and Li, Tanda and Liu, Kang and Liu, Zhie and Ge, Zhishuai},
  pages = {8},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Tian et al. - Asteroseismic analysis of solar-like star KIC 6225.pdf}
}

