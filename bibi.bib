  doi = {10.1088/1674-4527/17/5/44},
  author = {Li, Ya-Guang and Du, Ming-Hao and Xie, Bo-Han and Tian, Zhi-Jia and Bi, Shao-Lan and Li, Tan-Da and Wu, Ya-Qian and Liu, Kang},
  month = apr,
  year = {2017},
  pages = {044},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Li et al. - 2017 - Seismic diagnostics of solar-like oscillating star.pdf}
}

@article{li_asteroseismology_2018,
  title = {Asteroseismology of {{KIC}} 7107778: A Binary Comprising Almost Identical Subgiants},
  volume = {476},
  issn = {0035-8711, 1365-2966},
  shorttitle = {Asteroseismology of {{KIC}} 7107778},
  abstract = {We analyse an asteroseismic binary system: KIC 7107778, a non-eclipsing, unresolved target, with solar-like oscillations in both components. We used Kepler short cadence time series spanning nearly 2 yr to obtain the power spectrum. Oscillation mode parameters were determined using Bayesian inference and a nested sampling Monte Carlo algorithm with the DIAMONDS package. The power profiles of the two components fully overlap, indicating their close similarity. We modelled the two stars with MESA and calculated oscillation frequencies with GYRE. Stellar fundamental parameters (mass, radius, and age) were estimated by grid modelling with atmospheric parameters and the oscillation frequencies of l = 0, 2 modes as constraints. Most l = 1 mixed modes were identified with models searched using a bisection method. Stellar parameters for the two sub-giant stars are MA = 1.42 {$\pm$} 0.06 M , MB = 1.39 {$\pm$} 0.03 M , RA = 2.93 {$\pm$} 0.05 R , RB = 2.76 {$\pm$} 0.04 R , tA = 3.32 {$\pm$} 0.54 Gyr and tB = 3.51 {$\pm$} 0.33 Gyr. The mass difference of the system is {$\sim$}1 per cent. The results confirm their simultaneous birth and evolution, as is expected from binary formation. KIC 7107778 comprises almost identical twins, and is the first asteroseismic sub-giant binary to be detected.},
  language = {en},
  number = {1},
  journal = {Monthly Notices of the Royal Astronomical Society},
  doi = {10.1093/mnras/sty222},
  author = {Li, Yaguang and Bedding, Timothy R and Li, Tanda and Bi, Shaolan and Murphy, Simon J and Corsaro, Enrico and Chen, Li and Tian, Zhijia},
  month = may,
  year = {2018},
  pages = {470-481},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Li et al. - 2018 - Asteroseismology of KIC 7107778 a binary comprisi.pdf}
