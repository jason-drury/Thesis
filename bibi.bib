  pages = {L37},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Metcalfe et al. - 2015 - ASTEROSEISMIC MODELING OF 16 Cyg A & B USING THE C.pdf}
}

@article{metcalfe_stellar_2016,
