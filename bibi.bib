@article{silva_aguirre_stellar_2013,
  title = {{{STELLAR AGES AND CONVECTIVE CORES IN FIELD MAIN}}-{{SEQUENCE STARS}}: {{FIRST ASTEROSEISMIC APPLICATION TO TWO}} {{{\emph{KEPLER}}}} {{TARGETS}}},
  shorttitle = {{{STELLAR AGES AND CONVECTIVE CORES IN FIELD MAIN}}-{{SEQUENCE STARS}}},
  author = {Silva Aguirre, V. and Basu, S. and Brand{\~a}o, I. M. and {Christensen-Dalsgaard}, J. and Deheuvels, S. and Do{\u g}an, G. and Metcalfe, T. S. and Serenelli, A. M. and Ballot, J. and Chaplin, W. J. and Cunha, M. S. and Weiss, A. and Appourchaux, T. and Casagrande, L. and Cassisi, S. and Creevey, O. L. and Garc{\'i}a, R. A. and Lebreton, Y. and Noels, A. and Sousa, S. G. and Stello, D. and White, T. R. and Kawaler, S. D and Kjeldsen, H.},
  year = {2013},
  month = may,
  volume = {769},
  pages = {141},
  issn = {0004-637X, 1538-4357},
  doi = {10.1088/0004-637X/769/2/141},
  abstract = {Using asteroseismic data and stellar evolution models we obtain the first detection of a convective core in a Kepler field main-sequence star, putting a stringent constraint on the total size of the mixed zone and showing that extra mixing beyond the formal convective boundary exists. In a slightly less massive target the presence of a convective core cannot be conclusively discarded, and thus its remaining main-sequence lifetime is uncertain. Our results reveal that best-fit models found solely by matching individual frequencies of oscillations corrected for surface effects do not always properly reproduce frequency combinations. Moreover, slightly different criteria to define what the best-fit model is can lead to solutions with similar global properties but very different interior structures. We argue that the use of frequency ratios is a more reliable way to obtain accurate stellar parameters, and show that our analysis in field main-sequence stars can yield an overall precision of 1.5\%, 4\%, and 10\% in radius, mass, and age, respectively. We compare our results with those obtained from global oscillation properties, and discuss the possible sources of uncertainties in asteroseismic stellar modeling where further studies are still needed.},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Silva Aguirre et al. - 2013 - STELLAR AGES AND CONVECTIVE CORES IN FIELD MAIN-SE.pdf},
  journal = {The Astrophysical Journal},
  language = {en},
  number = {2}
}

@article{silva_aguirre_verifying_2012,
  title = {{{VERIFYING ASTEROSEISMICALLY DETERMINED PARAMETERS OF}} {{{\emph{KEPLER}}}} {{STARS USING}} {{{\emph{HIPPARCOS}}}} {{PARALLAXES}}: {{SELF}}-{{CONSISTENT STELLAR PROPERTIES AND DISTANCES}}},
  shorttitle = {{{VERIFYING ASTEROSEISMICALLY DETERMINED PARAMETERS OF}} {{{\emph{KEPLER}}}} {{STARS USING}} {{{\emph{HIPPARCOS}}}} {{PARALLAXES}}},
  author = {Silva Aguirre, V. and Casagrande, L. and Basu, S. and Campante, T. L. and Chaplin, W. J. and Huber, D. and Miglio, A. and Serenelli, A. M. and Ballot, J. and Bedding, T. R. and {Christensen-Dalsgaard}, J. and Creevey, O. L. and Elsworth, Y. and Garc{\'i}a, R. A. and Gilliland, R. L. and Hekker, S. and Kjeldsen, H. and Mathur, S. and Metcalfe, T. S. and Monteiro, M. J. P. F. G. and Mosser, B. and Pinsonneault, M. H. and Stello, D. and Weiss, A. and Tenenbaum, P. and Twicken, J. D. and Uddin, K.},
  year = {2012},
  month = sep,
  volume = {757},
  pages = {99},
  issn = {0004-637X, 1538-4357},
  doi = {10.1088/0004-637X/757/1/99},
  abstract = {Accurately determining the properties of stars is of prime importance for characterizing stellar populations in our Galaxy. The field of asteroseismology has been thought to be particularly successful in such an endeavor for stars in different evolutionary stages. However, to fully exploit its potential, robust methods for estimating stellar parameters are required and independent verification of the results is mandatory. With this purpose, we present a new technique to obtain stellar properties by coupling asteroseismic analysis with the InfraRed Flux Method. By using two global seismic observables and multi-band photometry, the technique allows us to obtain masses, radii, effective temperatures, bolometric fluxes, and hence distances for field stars in a self-consistent manner. We apply our method to 22 solar-like oscillators in the Kepler short-cadence sample, that have accurate Hipparcos parallaxes. Our distance determinations agree to better than 5\%, while measurements of spectroscopic effective temperatures and interferometric radii also validate our results. We briefly discuss the potential of our technique for stellar population analysis and models of Galactic Chemical Evolution.},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Silva Aguirre et al. - 2012 - VERIFYING ASTEROSEISMICALLY DETERMINED PARAMETERS .pdf},
  journal = {The Astrophysical Journal},
  language = {en},
  number = {1}
}

@article{silverberg_kepler_2016,
