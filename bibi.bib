}

@phdthesis{white_asteroseismology_2013,
  title = {Asteroseismology of Sun-like Stars with {{Kepler}}},
  author = {White, Timothy},
  year = {2013},
  month = jul,
  abstract = {Asteroseismology is the study of oscillations in stars. Oscillation properties depend on the interior of the star and so probe the unseen depths. Asteroseismology therefore has an important role in understanding stellar physics. By determining oscillation frequencies, stellar properties such as mass, radius and age can be deduced, which are required for galactic population studies. A revolution in asteroseismology is taking place due to the Kepler Space Telescope. Kepler observations have increased the number of stars with detected solar-like oscillations by more than an order of magnitude, allowing for ensemble asteroseismology. Using a grid of stellar models we determined the values of several asteroseismic parameters. These parameters vary with stellar properties, thereby constraining stellar mass and age. We make comparisons between these models and the Kepler ensemble. Observations and models broadly agree, but interesting discrepancies highlight current deficiencies in stellar models. Additionally, the values of these parameters can aid in correctly identifying the angular degree of modes. The asteroseismic parameter {$\Delta\nu$} is expected to scale with the square-root of the mean stellar density. We investigate this scaling relation in models and observations. We find that a temperature-dependent departure exists, which has implications for masses and radii derived from this relation. As well as studying stars as an ensemble, interesting single stars provide insights. We investigate mode amplitude variability in the rapidly-oscillating Ap star HR 1217 using a time-frequency analysis, revealing unexpected behaviour. The science output of asteroseismology can be maximized with complementary measurements using other techniques. Using long-baseline optical interferometry we directly measured the radii of three of the brightest Kepler stars. These direct measurements, in conjunction with asteroseismic measurements, will provide strong constraints for modelling of these stars.},
  annotation = {Accepted: 2014-04-14},
  copyright = {The author retains copyright of this thesis. It may only be used for the purposes of research and study.  It must not be used for any other purposes and may not be transmitted or shared with others without prior permission.},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/White_2013_Asteroseismology of sun-like stars with Kepler.pdf;/home/jdru4706/Zotero/storage/Q8G5YXFK/10392.html},
  language = {en},
  school = {University of Sydney}
}

@article{white_calculating_2011,
