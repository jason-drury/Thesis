  month = sep,
  year = {2017},
  keywords = {open clusters and associations: general,stars: fundamental parameters,stars: oscillations,methods: numerical,stars: late-type,methods: statistical},
  pages = {A3},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/2U4BEGEQ/Corsaro et al. - 2017 - Metallicity effect on stellar granulation detected.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/YURL54QY/Corsaro et al. - 2017 - Metallicity effect on stellar granulation detected.pdf}
