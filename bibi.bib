  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Carrera et al_2019_Open clusters in APOGEE and GALAH.pdf;/home/jdru4706/Dropbox_USyd/Apps/Zotero/Carrera et al_2019_Open clusters in APOGEE and GALAH2.pdf;/home/jdru4706/Dropbox_USyd/Apps/Zotero/Carrera et al_2019_Open clusters in APOGEE and GALAH3.pdf;/home/jdru4706/Zotero/storage/3L6CKJXK/abstract.html;/home/jdru4706/Zotero/storage/TQ4RHEGS/abstract.html;/home/jdru4706/Zotero/storage/VTI5HINN/abstract.html},
