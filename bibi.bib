@article{fuller_accelerated_2017,
  title = {Accelerated Tidal Circularization via Resonance Locking in {{KIC}} 8164262},
  author = {Fuller, Jim and Hambleton, Kelly and Shporer, Avi and Isaacson, Howard and Thompson, Susan},
  year = {2017},
  month = nov,
  volume = {472},
  pages = {L25-L29},
  issn = {1745-3925, 1745-3933},
  doi = {10.1093/mnrasl/slx130},
  abstract = {Tidal dissipation in binary star and planetary systems is poorly understood. Fortunately, eccentric binaries known as heartbeat stars often exhibit tidally excited oscillations, providing observable diagnostics of tidal circularization mechanisms and time-scales. We apply tidal theories to observations of the heartbeat star KIC 8164262, which contains an F-type primary in a very eccentric orbit that exhibits a prominent tidally excited oscillation. We demonstrate that the prominent oscillation is unlikely to result from a chance resonance between tidal forcing and a stellar oscillation mode. However, the oscillation has a frequency and amplitude consistent with the prediction of resonance locking, a mechanism in which coupled stellar and orbital evolution maintain a stable resonance between tidal forcing and a stellar oscillation mode. The resonantly excited mode produces efficient tidal dissipation (corresponding to an effective tidal quality factor Q {$\sim$} 5 \texttimes{} 104), such that tidal orbital decay/circularization proceeds on a stellar evolution time-scale.},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Fuller et al. - 2017 - Accelerated tidal circularization via resonance lo.pdf},
  journal = {Monthly Notices of the Royal Astronomical Society: Letters},
  language = {en},
  number = {1}
}

