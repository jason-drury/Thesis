}

@article{stello_non-radial_2014,
  title = {{{NON}}-{{RADIAL OSCILLATIONS IN M}}-{{GIANT SEMI}}-{{REGULAR VARIABLES}}: {{STELLAR MODELS AND}} {{{\emph{KEPLER}}}} {{OBSERVATIONS}}},
  shorttitle = {{{NON}}-{{RADIAL OSCILLATIONS IN M}}-{{GIANT SEMI}}-{{REGULAR VARIABLES}}},
