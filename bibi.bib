  author = {Casagrande, L. and Aguirre, V. Silva and Stello, D. and Huber, D. and Serenelli, A. M. and Cassisi, S. and Dotter, A. and Milone, A. P. and Hodgkin, S. and Marino, A. F. and Lund, M. N. and Pietrinferni, A. and Asplund, M. and Feltzing, S. and Flynn, C. and Grundahl, F. and Nissen, P. E. and Sch{\"o}nrich, R. and Schlesinger, K. J. and Wang, W.},
