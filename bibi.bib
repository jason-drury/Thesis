}

@article{jiang_modeling_2011,
  title = {{{MODELING}} {{{\emph{KEPLER}}}} {{OBSERVATIONS OF SOLAR}}-{{LIKE OSCILLATIONS IN THE RED GIANT STAR HD}} 186355},
  volume = {742},
  issn = {0004-637X, 1538-4357},
  abstract = {We have analyzed oscillations of the red giant star HD 186355 observed by the NASA Kepler satellite. The data consist of the first five quarters of science operations of Kepler, which cover about 13 months. The high-precision time-series data allow us to accurately extract the oscillation frequencies from the power spectrum. We find that the frequency of the maximum oscillation power, {$\nu$}max, and the mean large frequency separation, {$\Delta\nu$}, are around 106 and 9.4 {$\mu$}Hz, respectively. A regular pattern of radial and non-radial oscillation modes is identified by stacking the power spectra in an echelle diagram. We use the scaling relations of {$\Delta\nu$} and {$\nu$}max to estimate the preliminary asteroseismic mass, which is confirmed with the modeling result (M = 1.45 {$\pm$} 0.05 M ) using the Yale Rotating stellar Evolution Code (YREC7). In addition, we constrain the effective temperature, luminosity, and radius from comparisons between observational constraints and models. A number of mixed l = 1 modes are also detected and taken into account in our model comparisons. We find a mean observational period spacing for these mixed modes of about 58 s, suggesting that this red giant branch star is in the shell hydrogen-burning phase.},
  language = {en},
  number = {2},
  journal = {The Astrophysical Journal},
  doi = {10.1088/0004-637X/742/2/120},
  author = {Jiang, C. and Jiang, B. W. and {Christensen-Dalsgaard}, J. and Bedding, T. R. and Stello, D. and Huber, D. and Frandsen, S. and Kjeldsen, H. and Karoff, C. and Mosser, B. and Demarque, P. and Fanelli, M. N. and Kinemuchi, K. and Mullally, F.},
  month = dec,
  year = {2011},
  pages = {120},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Jiang et al. - 2011 - MODELING iKEPLERi OBSERVATIONS OF SOLAR-LIKE .pdf}
}

@article{jiang_using_2017,
  title = {Using the Phase Shift to Asymptotically Characterize the Dipolar Mixed Modes in Post-Main-Sequence Stars},
  issn = {0035-8711, 1365-2966},
  abstract = {Mixed modes have been extensively observed in post-main-sequence stars by the Kepler and CoRoT space missions. The mixture of the p and g modes can be measured by the dimensionless coefficient q, the so-called coupling strength factor. In this paper, we discuss the utility of the phase shifts \texttheta{} from the eigenvalue condition for mixed modes as a tool to characterize dipolar mixed modes from the theoretical as well as the practical point of view. Unlike the coupling strength, whose variation in a given star is very small over the relevant frequency range, the phase shifts vary significantly for different modes. The analysis in terms of \texttheta{} can also provide a better understanding of the pressure and gravity radial order for a given mixed mode. Observed frequencies of the Kepler red-giant star KIC 3744043 are used to test the method. The results are very promising.},
  language = {en},
