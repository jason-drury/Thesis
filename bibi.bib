@article{benko_flavours_2010,
  title = {Flavours of Variability: 29 {{RR Lyrae}} Stars Observed with {{Kepler}}: {{RR Lyrae}} Stars Observed with {{Kepler}}},
  shorttitle = {Flavours of Variability},
  author = {Benk{\H o}, J. M. and Kolenberg, K. and Szab{\'o}, R. and Kurtz, D. W. and Bryson, S. and Bregman, J. and Still, M. and Smolec, R. and Nuspl, J. and Nemec, J. M. and Moskalik, P. and Kopacki, G. and Koll{\'a}th, Z. and Guggenberger, E. and Di Criscienzo, M. and {Christensen-Dalsgaard}, J. and Kjeldsen, H. and Borucki, W. J. and Koch, D. and Jenkins, J. M. and Van Cleve, J. E.},
