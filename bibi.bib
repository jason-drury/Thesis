  author = {Lagarde, N. and Robin, A. C. and Reyl{\'e}, C. and Nasello, G.},
  month = may,
  year = {2017},
  pages = {A27},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/TXFGAMPN/Lagarde et al. - 2017 - Population synthesis to constrain Galactic and ste.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/YGF96MQS/aa30253-16.html}
