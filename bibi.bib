@article{cantiello_angular_2014,
  title = {{{ANGULAR MOMENTUM TRANSPORT WITHIN EVOLVED LOW}}-{{MASS STARS}}},
  volume = {788},
  issn = {0004-637X, 1538-4357},
  abstract = {Asteroseismology of 1.0\textendash{}2.0 M red giants by the Kepler satellite has enabled the first definitive measurements of interior rotation in both first ascent red giant branch (RGB) stars and those on the helium burning clump. The inferred rotation rates are 10\textendash{}30 days for the {$\approx$}0.2 M He degenerate cores on the RGB and 30\textendash{}100 days for the He burning core in a clump star. Using the Modules for Experiments in Stellar Evolution code, we calculate state-of-the-art stellar evolution models of low mass rotating stars from the zero-age main sequence to the cooling white dwarf (WD) stage. We include transport of angular momentum due to rotationally induced instabilities and circulations, as well as magnetic fields in radiative zones (generated by the Tayler\textendash{}Spruit dynamo). We find that all models fail to predict core rotation as slow as observed on the RGB and during core He burning, implying that an unmodeled angular momentum transport process must be operating on the early RGB of low mass stars. Later evolution of the star from the He burning clump to the cooling WD phase appears to be at nearly constant core angular momentum. We also incorporate the adiabatic pulsation code, ADIPLS, to explicitly highlight this shortfall when applied to a specific Kepler asteroseismic target, KIC8366239.},
  language = {en},
  number = {1},
  journal = {The Astrophysical Journal},
  doi = {10.1088/0004-637X/788/1/93},
  author = {Cantiello, Matteo and Mankovich, Christopher and Bildsten, Lars and {Christensen-Dalsgaard}, J{\o}rgen and Paxton, Bill},
  month = may,
  year = {2014},
  pages = {93},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Cantiello et al. - 2014 - ANGULAR MOMENTUM TRANSPORT WITHIN EVOLVED LOW-MASS.pdf}
}

