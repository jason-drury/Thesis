  shorttitle = {Regularities in Frequency Spacings of {$\delta$} {{Scuti}} Stars},
  author = {Breger, M. and Balona, L. and Lenz, P. and Hollek, J. K. and Kurtz, D. W. and Catanzaro, G. and Marconi, M. and Pamyatnykh, A. A. and Smalley, B. and Su{\'a}rez, J. C. and Szabo, R. and Uytterhoeven, K. and Ripepi, V. and {Christensen-Dalsgaard}, J. and Kjeldsen, H. and Fanelli, M. N. and Ibrahim, K. A. and Uddin, K.},
  year = {2011},
  month = jun,
  volume = {414},
