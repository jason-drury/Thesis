  month = jan,
  year = {2012},
  pages = {55-57},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/P7IQN9L8/Beck et al. - 2012 - Fast core rotation in red-giant stars as revealed .pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/UFFW8VCC/nature10612.html}
