  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Sun et al_2019_Stellar rotation and the extended main sequence turnoff in the open cluster NGC.pdf;/home/jdru4706/Zotero/storage/H7B53QRD/1904.html},
