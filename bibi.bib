@article{borkovits_dynamical_2013,
  title = {Dynamical Masses, Absolute Radii and {{3D}} Orbits of the Triply Eclipsing Star {{HD}} 181068 from {{Kepler}} Photometry},
  author = {Borkovits, T. and Derekas, A. and Kiss, L. L. and Kir{\'a}ly, A. and {Forg{\'a}cs-Dajka}, E. and B{\'i}r{\'o}, I. B. and Bedding, T. R. and Bryson, S. T. and Huber, D. and Szab{\'o}, R.},
