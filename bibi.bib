@article{masseron_spectroscopic_2017,
  title = {The Spectroscopic Indistinguishability of Red Giant Branch and Red Clump Stars},
  volume = {597},
  copyright = {\textcopyright{} ESO, 2016},
  issn = {0004-6361, 1432-0746},
  abstract = {\emph{Context. \emph{Stellar spectroscopy provides useful information on the physical properties of stars such as effective temperature, metallicity and surface gravity. However, those photospheric characteristics are often hampered by systematic uncertainties. The joint spectro-sismo project (APOGEE+\emph{Kepler\emph{, aka APOKASC) of field red giants has revealed a puzzling offset between the surface gravities (log \emph{g\emph{) determined spectroscopically and those determined using asteroseismology, which is largely dependent on the stellar evolutionary status. \emph{Aims. \emph{Therefore, in this letter, we aim to shed light on the spectroscopic source of the offset.\emph{Methods. \emph{We used the APOKASC sample to analyse the dependencies of the log \emph{g\emph{ discrepancy as a function of stellar mass and stellar evolutionary status. We discuss and study the impact of some neglected abundances on spectral analysis of red giants, such as He and carbon isotopic ratio. \emph{Results. \emph{We first show that, for stars at the bottom of the red giant branch where the first dredge-up had occurred, the discrepancy between spectroscopic log \emph{g\emph{ and asteroseismic log \emph{g\emph{ depends on stellar mass. This seems to indicate that the log \emph{g\emph{ discrepancy is related to CN cycling. Among the CN-cycled elements, we demonstrate that the carbon isotopic ratio (\textsuperscript{12\textsuperscript{C /\textsuperscript{13\textsuperscript{C) has the largest impact on stellar spectrum. In parallel, we observe that this log \emph{g\emph{ discrepancy shows a similar trend as the \textsuperscript{12\textsuperscript{C /\textsuperscript{13\textsuperscript{C ratios as expected by stellar evolution theory. Although we did not detect a direct spectroscopic signature of \textsuperscript{13\textsuperscript{C, other corroborating evidences suggest that the discrepancy in log \emph{g\emph{ is tightly correlated to the production of \textsuperscript{13\textsuperscript{C in red giants. Moreover, by running the data-driven algorithm (the Cannon) on a synthetic grid trained on the APOGEE data, we try to evaluate more quantitatively the impact of various \textsuperscript{12\textsuperscript{C /\textsuperscript{13\textsuperscript{C ratios. \emph{Conclusions. \emph{While we have demonstrated that \textsuperscript{13\textsuperscript{C indeed impacts all parameters, the size of the impact is smaller than the observed offset in log \emph{g\emph{. If further tests confirm that \textsuperscript{13\textsuperscript{C is not the main element responsible of the log \emph{g\emph{ problem, the number of spectroscopic effects remaining to be investigated is now relatively limited (if any).}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}}},
  language = {en},
  journal = {A\&A},
  doi = {10.1051/0004-6361/201629938},
  author = {Masseron, T. and Hawkins, K.},
  month = jan,
  year = {2017},
  pages = {L3},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/C2NLMTR2/Masseron and Hawkins - 2017 - The spectroscopic indistinguishability of red gian.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/4MSD79Z7/aa29938-16.html}
}

