  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Solar and Stellar Astrophysics},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/J5QWS4LT/Leão et al. - 2018 - Spectroscopic and astrometric radial velocities H.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/QMZ9E4J4/1811.html},
