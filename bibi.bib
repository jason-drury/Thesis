@article{basri_photometric_2011,
  title = {{{PHOTOMETRIC VARIABILITY IN}} {{{\emph{KEPLER}}}} {{TARGET STARS}}. {{II}}. {{AN OVERVIEW OF AMPLITUDE}}, {{PERIODICITY}}, {{AND ROTATION IN FIRST QUARTER DATA}}},
  author = {Basri, Gibor and Walkowicz, Lucianne M. and Batalha, Natalie and Gilliland, Ronald L. and Jenkins, Jon and Borucki, William J. and Koch, David and Caldwell, Doug and Dupree, Andrea K. and Latham, David W. and Marcy, Geoffrey W. and Meibom, Soeren and Brown, Tim},
