  keywords = {Astrophysics - Astrophysics of Galaxies},
  pages = {1095-1107},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/LW8W6YFA/Bostanci et al. - 2015 - A comprehensive study of the open cluster NGC 6866.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/AWWWTRYS/1507.html},
  annote = {Comment: 14 pages, including 16 figures and 7 tables, accepted for publication in MNRAS. Table 4 in the manuscript will be published electronically}
}

@article{janes_open_2014-1,
