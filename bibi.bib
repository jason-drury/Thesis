  issn = {0004-6361, 1432-0746},
  abstract = {In recent years the global seismic scaling relations for the frequency of maximum power and for the large frequency separation have caught the attention of various fields of astrophysics. With the exquisite photometry of \textbackslash{}textit\{Kepler\}, the uncertainties in the seismic observables are small enough to estimate masses and radii with a precision of only a few per cent. Even though this seems to work quite well for main-sequence stars, there is empirical evidence, mainly from studies of eclipsing binary systems, that the seismic scaling relations overestimate the mass and radius of red giants by about 15 and 5\textbackslash\%, respectively. Model-based corrections of the \$\textbackslash{}Delta\textbackslash{}nu -\$scaling reduce the problem but do not solve it. We re-examine the global oscillation parameters of the giants in the binary systems in order to determine their seismic fundamental parameters and find them to agree with the dynamic parameters from the literature if we adopt nonlinear scalings. We note that a curvature and glitch corrected \$\textbackslash{}Delta\textbackslash{}nu\_\textbackslash{}mathrm\{cor\}\$ should be preferred over a local or average values. We then compare the observed seismic parameters of the cluster giants to those scaled from independent measurements and find the same nonlinear behaviour as for the eclipsing binaries. Our final proposed scaling relations are based on both samples and cover a broad range of evolutionary stages from RGB to RC stars: \$g/\textbackslash{}sqrt\{T\_\textbackslash{}mathrm\{eff\}\} = (\textbackslash{}nu\_\textbackslash{}mathrm\{max\}/\textbackslash{}nu\_\textbackslash{}mathrm\{max,\textbackslash{}odot\})\^\{1.0075\textbackslash{}pm0.0021\}\$ and \$\textbackslash{}sqrt\{\textbackslash{}bar\textbackslash{}rho\} = (\textbackslash{}Delta\textbackslash{}nu\_\textbackslash{}mathrm\{cor\}/\textbackslash{}Delta\textbackslash{}nu\_\textbackslash{}mathrm\{cor,\textbackslash{}odot\})[\textbackslash{}eta - (0.0085\textbackslash{}pm0.0025) \textbackslash{}log\^2 (\textbackslash{}Delta\textbackslash{}nu\_\textbackslash{}mathrm\{cor\}/\textbackslash{}Delta\textbackslash{}nu\_\textbackslash{}mathrm\{cor,\textbackslash{}odot\})]\^\{-1\}\$, where \$g\$, \$T\_\textbackslash{}mathrm\{eff\}\$, and \$\textbackslash{}bar\textbackslash{}rho\$ are in solar units, \$\textbackslash{}nu\_\textbackslash{}mathrm\{max,\textbackslash{}odot\}=3140\textbackslash{}pm5\textbackslash{}mu\$Hz and \$\textbackslash{}Delta\textbackslash{}nu\_\textbackslash{}mathrm\{cor,\textbackslash{}odot\}=135.08\textbackslash{}pm0.02\textbackslash{}mu\$Hz , and \$\textbackslash{}eta\$ is equal to one in case of RGB stars and \$1.04\textbackslash{}pm0.01\$ for RC stars.},
  journal = {Astronomy \& Astrophysics},
  doi = {10.1051/0004-6361/201832831},
  author = {Kallinger, T. and Beck, P. G. and Stello, D. and Garcia, R. A.},
  month = aug,
  year = {2018},
  keywords = {Astrophysics - Solar and Stellar Astrophysics},
