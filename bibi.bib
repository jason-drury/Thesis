  eprinttype = {arxiv},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Vasiliev_2018_Systematic errors in Gaia DR2 astrometry and their impact on measurements of.pdf;/home/jdru4706/Zotero/storage/RLNACJHF/1811.html},
  journal = {arXiv:1811.05345 [astro-ph]},
  keywords = {Astrophysics - Astrophysics of Galaxies},
  note = {Comment: to be submitted to MNRAS; comments welcome! the code for dealing with correlated systematic errors is provided at https://github.com/GalacticDynamics-Oxford/GaiaTools},
