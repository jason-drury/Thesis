  month = sep,
  year = {2006},
  pages = {607},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/SMTAA3C2/Croll et al. - 2006 - Differential Rotation of ∊ Eridani Detected by MOS.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/5M9YG9EF/abstract.html}
