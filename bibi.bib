@article{murphy_super-nyquist_2013,
  title = {Super-{{Nyquist}} Asteroseismology with the {{Kepler Space Telescope}}},
  volume = {430},
  issn = {1365-2966, 0035-8711},
  abstract = {Barycentric corrections made to the timing of Kepler observations, necessitated by variations in light arrival time at the satellite, break the regular time-sampling of the data \textendash{} the time stamps are periodically modulated. A consequence is that Nyquist aliases are split into multiplets that can be identified by their shape. Real pulsation frequencies are distinguishable from these aliases and their frequencies are completely recoverable, even in the super-Nyquist regime, i.e. when the sampling interval is longer than half the pulsation period. We provide an analytical derivation of the phenomenon, alongside demonstrations with simulated and real Kepler data for {$\delta$} Sct, roAp and sdBV stars. For Kepler data sets spanning more than one Kepler orbital period (372.5 d), there are no Nyquist ambiguities on the determination of pulsation frequencies, which are the fundamental data of asteroseismology.},
  language = {en},
  number = {4},
  journal = {Monthly Notices of the Royal Astronomical Society},
  doi = {10.1093/mnras/stt105},
  author = {Murphy, Simon J. and Shibahashi, Hiromoto and Kurtz, Donald W.},
  month = apr,
  year = {2013},
  pages = {2986-2998},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Murphy et al. - 2013 - Super-Nyquist asteroseismology with the Kepler Spa.pdf}
}

