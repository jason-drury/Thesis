  month = jan,
  year = {2013},
  pages = {1656-1672},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Borkovits et al. - 2013 - Dynamical masses, absolute radii and 3D orbits of .pdf}
}

