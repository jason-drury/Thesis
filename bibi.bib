  month = oct,
  year = {2016},
  keywords = {Astrophysics - Solar and Stellar Astrophysics},
  pages = {11},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/2ZFT647B/Sandquist et al. - 2016 - The Age and Distance of the Kepler Open Cluster NG.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/HPGZE3YE/1608.html},
