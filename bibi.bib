  pages = {1223-1233},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Kurtz et al. - 2015 - Validation of the frequency modulation technique a.pdf}
}

@article{kurtz_asteroseismic_2014,
