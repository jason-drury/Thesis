  issn = {1538-4357},
  doi = {10.3847/1538-4357/aaedc1},
  abstract = {We present photometric analysis of twelve Galactic open clusters and show that the same multiple-population phenomenon observed in Magellanic Clouds (MCs) is present in nearby open clusters. Nearly all the clusters younger than \$\textbackslash sim\$2.5 Gyr of both MCs exhibit extended main-sequence turnoffs (eMSTOs) and all the cluster younger than \$\textbackslash sim\$700 Myr show broadened/split main sequences (MSs). High-resolution spectroscopy has revealed that these clusters host stars with a large spread in the observed projected rotations. In addition to rotation, internal age variation is indicated as a possible responsible for the eMSTOs, making these systems the possible young counterparts of globular clusters with multiple populations. Recent work has shown that the eMSTO+broadened MSs are not a peculiarity of MCs clusters. Similar photometric features have been discovered in a few Galactic open clusters, challenging the idea that the color-magnitude diagrams (CMDs) of these systems are similar to single isochrones and opening new windows to explore the eMSTO phenomenon. We exploit photometry+proper motions from Gaia DR2 to investigate the CMDs of open clusters younger than \$\textbackslash sim\$1.5 Gyr. Our analysis suggests that: (i) twelve open clusters show eMSTOs and/or broadened MSs, that cannot be due neither to field contamination, nor binaries; (ii) split/broadened MSs are observed in clusters younger than \$\textbackslash sim\$700 Myr, while older objects display only an eMSTO, similarly to MCs clusters; (iii) the eMSTO, if interpreted as a pure age spread, increases with age, following the relation observed in MCs clusters and demonstrating that rotation is the responsible for this phenomenon.},
  archivePrefix = {arXiv},
  eprint = {1811.01192},
  eprinttype = {arxiv},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Cordoni et al_2018_Extended main sequence turnoff as a common feature of Milky Way open clusters.pdf;/home/jdru4706/Zotero/storage/AD5KBXR8/1811.html},
  journal = {The Astrophysical Journal},
  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Solar and Stellar Astrophysics},
  note = {Comment: 17 pages, 42 figures, 1 table, accepted for publication in ApJ (31/10/2018)},
  number = {2}
}

