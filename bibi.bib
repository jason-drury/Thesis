  pages = {1553},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/9XG7F3WT/Derekas et al. - 2017 - The Kepler Cepheid V1154 Cyg revisited light curv.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/YX5PIUS2/abstract.html}
