  month = dec,
  year = {2010},
  pages = {1509-1517},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Kawaler et al. - 2010 - First Kepler results on compact pulsators - V. Slo.pdf}
}

@article{keen_kic_2015,
