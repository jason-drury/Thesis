  journal = {arXiv:1811.08771 [astro-ph]},
  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Solar and Stellar Astrophysics},
  note = {Comment: 18 pages, 9 figures, accepted for publication in MNRAS},
