  doi = {10.3847/1538-3881/aacb21},
  author = {{Bailer-Jones}, C. A. L. and Rybizki, J. and Fouesneau, M. and Mantelet, G. and Andrae, R.},
  month = jul,
  year = {2018},
  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Solar and Stellar Astrophysics,Astrophysics - Instrumentation and Methods for Astrophysics},
  pages = {58},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/8W2E4CJ3/Bailer-Jones et al. - 2018 - Estimating distances from parallaxes IV Distances.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/8EHFZFGT/1804.html},
