  file = {/home/jdru4706/Zotero/storage/KD624W9Q/Montet et al. - 2020 - The Young Planet DS Tuc Ab has a Low Obliquity.pdf;/home/jdru4706/Zotero/storage/587QM4KH/1912.html},
  journal = {AJ},
  keywords = {Astrophysics - Earth and Planetary Astrophysics,Astrophysics - Solar and Stellar Astrophysics},
  note = {Comment: 18 pages, four figures, AJ accepted},
  number = {3}
}

