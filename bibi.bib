@article{garcia_saravia_ortiz_de_montellano_automated_2018,
  title = {Automated Asteroseismic Peak Detections},
  volume = {476},
  issn = {0035-8711, 1365-2966},
  abstract = {Space observatories such as Kepler have provided data that can potentially revolutionize our understanding of stars. Through detailed asteroseismic analyses we are capable of determining fundamental stellar parameters and reveal the stellar internal structure with unprecedented accuracy. However, such detailed analyses, known as peak bagging, have so far been obtained for only a small percentage of the observed stars while most of the scientific potential of the available data remains unexplored. One of the major challenges in peak bagging is identifying how many solar-like oscillation modes are visible in a power density spectrum. Identification of oscillation modes is usually done by visual inspection that is time-consuming and has a degree of subjectivity. Here, we present a peak-detection algorithm especially suited for the detection of solar-like oscillations. It reliably characterizes the solar-like oscillations in a power density spectrum and estimates their parameters without human intervention. Furthermore, we provide a metric to characterize the false positive and false negative rates to provide further information about the reliability of a detected oscillation mode or the significance of a lack of detected oscillation modes. The algorithm presented here opens the possibility for detailed and automated peak bagging of the thousands of solar-like oscillators observed by Kepler.},
  language = {en},
  number = {2},
  journal = {Monthly Notices of the Royal Astronomical Society},
  doi = {10.1093/mnras/sty253},
  author = {{Garc{\'i}a Saravia Ortiz de Montellano}, Andr{\'e}s and Hekker, S and Theme{\ss}l, N},
  month = may,
  year = {2018},
  pages = {1470-1496},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/García Saravia Ortiz de Montellano et al. - 2018 - Automated asteroseismic peak detections.pdf}
}

