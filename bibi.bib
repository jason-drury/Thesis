  month = may,
  year = {2012},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/LL6P42GA/Corsaro et al. - 2012 - Asteroseismology of the open clusters NGC 6791, NG.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/J544E2MG/1205.html}
