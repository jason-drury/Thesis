  file = {/home/jdru4706/Zotero/storage/LLY9U5D2/Bostancı et al. - 2015 - A comprehensive study of the open cluster NGC 6866.pdf},
  journal = {Monthly Notices of the Royal Astronomical Society},
  keywords = {Hertzsprung-Russell and colour-magnitude diagrams,open clusters and associations: individual: NGC 6866}
}

