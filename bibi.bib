  author = {Tayar, Jamie and Somers, Garrett and Pinsonneault, Marc H. and Stello, Dennis and Mints, Alexey and Johnson, Jennifer A. and {O. Zamora} and {Garc{\'i}a-Hern{\'a}ndez}, D. A. and Maraston, Claudia and Serenelli, Aldo and Prieto, Carlos Allende and Bastien, Fabienne A. and Basu, Sarbani and Bird, J. C. and Cohen, R. E. and Cunha, Katia and Elsworth, Yvonne and Garc{\'i}a, Rafael A. and {Leo Girardi} and Hekker, Saskia and Holtzman, Jon and Huber, Daniel and Mathur, Savita and M{\'e}sz{\'a}ros, Szabolcs and Mosser, B. and {Matthew Shetrone} and Aguirre, Victor Silva and Stassun, Keivan and Stringfellow, Guy S. and Zasowski, Gail and {Roman-Lopes}, A.},
  year = {2017},
