  volume = {832},
  issn = {0004-637X},
  shorttitle = {{{KOI}}-1003},
  abstract = {Using the high-precision photometry from the Kepler space telescope, thousands of stars with stellar and planetary companions have been observed. The characterization of stars with companions is not always straightforward and can be contaminated by systematic and stellar influences on the light curves. Here, through a detailed analysis of starspots and eclipses, we identify KOI-1003 as a new, active RS CVn star\textemdash{}the first identified with data from Kepler . The Kepler light curve of this close binary system exhibits the system's primary transit, secondary eclipse, and starspot evolution of two persistent active longitudes. The near equality of the system's orbital and rotation periods indicates the orbit and primary star's rotation are nearly synchronized ( \#\#IMG\#\# [http://ej.iop.org/images/0004-637X/832/2/207/apjaa44e5ieqn1.gif] \$P\_\textbackslash{}mathrmorb=8.360613\textbackslash{}pm 0.000003\textbackslash,\textbackslash{}mathrmdays\$ ; \#\#IMG\#\# [http://ej.iop.org/images/0004-637X/832/2/207/apjaa44e5ieqn2.gif] \$P\_\textbackslash{}mathrmrot\textbackslash{}sim 8.23\textbackslash,\textbackslash{}mathrmdays\$ ). By assuming the secondary star is on the main sequence, we suggest the system consists of a \#\#IMG\#\# [http://ej.iop.org/images/0004-637X/832/2/207/apjaa44e5ieqn3.gif] \$1.45\_-0.19\^+0.11\textbackslash{} M\_{\o}dot \$ subgiant primary and a \#\#IMG\#\# [http://ej.iop.org/images/0004-637X/832/2/207/apjaa44e5ieqn4.gif] \$0.59\_-0.04\^+0.03\textbackslash{} M\_{\o}dot \$ main-sequence companion. Our work gives a distance of 4400 {$\pm$} 600 pc and an age of \#\#IMG\#\# [http://ej.iop.org/images/0004-637X/832/2/207/apjaa44e5ieqn5.gif] \$t=3.0\_+2.0\^-0.5\textbackslash,\textbackslash{}mathrmGyr\$ , parameters which are discrepant with previous studies that included the star as a member of the open cluster NGC 6791.},
  language = {en},
  number = {2},
  journal = {ApJ},
  doi = {10.3847/0004-637X/832/2/207},
  author = {Roettenbacher, Rachael M. and Kane, Stephen R. and Monnier, John D. and Harmon, Robert O.},
  year = {2016},
  keywords = {NGC 6791,Membership},
  pages = {207},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/J78NNWP9/Roettenbacher et al. - 2016 - KOI-1003 A NEW SPOTTED, ECLIPSING RS CVN BINARY I.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/YKUQ2454/Roettenbacher et al. - 2016 - KOI-1003 A New Spotted, Eclipsing RS CVn Binary i.pdf}
}

@article{milliman_wiyn_2016,
  title = {{{WIYN Open Cluster Study}}. {{LXXI}}. {{Spectroscopic Membership}} and {{Orbits}} of {{NGC}} 6791 {{Sub}}-Subgiants},
  volume = {151},
  issn = {1538-3881},
  abstract = {In an optical color\textendash{}magnitude diagram, sub-subgiants (SSGs) lie redward of the main sequence and fainter than the base of the red giant branch in a region not easily populated by standard stellar-evolution pathways. In this paper, we present multi-epoch radial velocities for five SSG candidates in the old and metal-rich open cluster NGC 6791 (8 Gyr, [Fe/H] = +0.30). From these data, we are able to make three-dimensional kinematic membership determinations and confirm four SSG candidates as likely cluster members. We also identify three member SSGs as short-period binary systems and present their orbital solutions. These are the first SSGs with known three-dimensional kinematic membership, binary status, and orbital parameters since the two SSGs in M67 studied by Mathieu et al. We also remark on the other properties of these stars including photometric variability, H {$\alpha$} emission, and X-ray luminosity. The membership confirmation of these SSGs in NGC 6791 strengthens the case that SSGs are a new class of nonstandard stellar evolution products, and that a physical mechanism must be found that explains the evolutionary paths of these stars.},
  language = {en},
  number = {6},
  journal = {AJ},
  doi = {10.3847/0004-6256/151/6/152},
  author = {Milliman, Katelyn E. and Leiner, Emily and Mathieu, Robert D. and Tofflemire, Benjamin M. and Platais, Imants},
  year = {2016},
  keywords = {NGC 6791,Membership},
  pages = {152},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/R9IL24QM/Milliman et al. - 2016 - WIYN OPEN CLUSTER STUDY. LXXI. SPECTROSCOPIC MEMBE.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/WTMJBHR8/Milliman et al. - 2016 - WIYN Open Cluster Study. LXXI. Spectroscopic Membe.pdf}
}

@article{mason_kepler_2016,
  title = {Kepler and {{Hale}} Observations of {{V523 Lyrae}}},
  volume = {589},
  copyright = {\textcopyright{} ESO, 2016},
  issn = {0004-6361, 1432-0746},
  abstract = {We present new observations of the cataclysmic variable (CV) V523 Lyr, a member of the open cluster NGC 6791. The \emph{Kepler \emph{Space telescope obtained photometric observations of this source and we examine the nearly three-year-long light curve. The observations show numerous small amplitude outbursts recurring on average every 33 d, intermittent quasi-periodic oscillations, and a significant fully coherent period of \textasciitilde{}3.8 h, which we identify as the orbital period of the binary. Contemporaneous optical spectroscopy of V523 Lyr reveals a faint blue source with broad Balmer absorption lines containing narrow emission cores. H\emph{{$\alpha$}\emph{ is in emission above the continuum. The low amplitude of the photometric signal and no detected velocity motion suggest a low orbital inclination. We discuss the properties of V523 Lyr and show that it is a member of the growing group of anomalous Z Cam type CVs, systems that show stunted outbursts, light curve standstills, and occasional deep drops in brightness.}}}}},
  language = {en},
  journal = {A\&A},
  doi = {10.1051/0004-6361/201628245},
  author = {Mason, E. and Howell, S. B.},
  month = may,
