@article{benomar_determination_2014,
  title = {Determination of Three-Dimensional Spin\textendash{}Orbit Angle with Joint Analysis of Asteroseismology, Transit Lightcurve, and the {{Rossiter}}\textendash{{McLaughlin}} Effect: {{Cases}} of {{HAT}}-{{P}}-7 and {{Kepler}}-25},
  volume = {66},
  issn = {2053-051X, 0004-6264},
  shorttitle = {Determination of Three-Dimensional Spin\textendash{}Orbit Angle with Joint Analysis of Asteroseismology, Transit Lightcurve, and the {{Rossiter}}\textendash{{McLaughlin}} Effect},
  abstract = {We develop a detailed methodology of determining three-dimensionally the angle between the stellar spin and the planetary orbit axis vectors, {$\psi$}, for transiting planetary systems. The determination of {$\psi$} requires the independent estimates of the inclination angles of the stellar spin axis and of the planetary orbital axis with respect to the line of sight, i and iorb, and the projection of the spin\textendash{}orbit angle on to the plane of the sky, {$\lambda$}. These are mainly derived from asteroseismology, transit lightcurve, and the Rossiter\textendash{}McLaughlin effect, respectively. The detailed joint analysis of those three datasets enables an accurate and precise determination of the numerous parameters characterizing the planetary system, in addition to {$\psi$}. We demonstrate the power of the joint analysis for the two specific systems HAT-P-7 and Kepler-25. HAT-P-7b is the first exoplanet suspected to be a retrograde (or polar) planet because of the significant misalignment {$\lambda$} {$\approx$} 180{$\smwhtcircle$}. Our joint analysis indicates i {$\approx$} 30{$\smwhtcircle$} and {$\psi$} {$\approx$} 120{$\smwhtcircle$}, suggesting that the planetary orbit is closer to polar rather than retrograde. Kepler-25 is one of the few multi-transiting planetary systems with measured {$\lambda$}, and hosts two short-period transiting planets and one outer non-transiting planet. The projected spin\textendash{}orbit angle of the larger transiting planet, Kepler-25c, has been measured to be {$\lambda$} {$\approx$} 0{$\smwhtcircle$}, implying that the system is well aligned. With the help of the tight constraint from asteroseismology, however, we obtain i = 65{$\smwhtcircle$}.4+-160{$\smwhtcircle$}. 4{$\smwhtcircle$}. 6 and {$\psi$} = 26{$\smwhtcircle$}. 9+-79 {$\smwhtcircle$}.},
  language = {en},
  number = {5},
  journal = {Publications of the Astronomical Society of Japan},
  doi = {10.1093/pasj/psu069},
  author = {Benomar, Othman and Masuda, Kento and Shibahashi, Hiromoto and Suto, Yasushi},
  month = oct,
  year = {2014},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Benomar et al. - 2014 - Determination of three-dimensional spin–orbit angl.pdf}
}

@article{benomar_asteroseismology_2014,
