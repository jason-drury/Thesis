  keywords = {Astrophysics - Astrophysics of Galaxies,Astrophysics - Solar and Stellar Astrophysics},
  pages = {A12},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/P7JJT57L/Wilkinson et al. - 2018 - New member candidates of Upper Scorpius from Gaia .pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/XTNM86GZ/1804.html},
