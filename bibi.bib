  author = {{Soares-Furtado}, M. and Hartman, J. D. and Bhatti, W. and Bouma, L. G. and Barna, T. and Bakos, G. A.},
  year = {2019},
  month = nov,
  abstract = {We present a catalog of 1,143 periodic variables, compiled from our image-subtracted photometric analysis of the K2 Campaign-0 super stamp. This super stamp is centered on the open clusters M35 and NGC 2158. Approximately 46\% of our periodic variables were previously unreported. Of the catalog variables, we find that 331 are members of M35 and 56 are members of NGC 2158 (\$\{P\_\{\textbackslash mathrm\{m\}\}\textasciitilde{$>\sptilde$}0.5\}\$). Our catalog contains two new transiting exoplanet candidates, both of which orbit field stars. The smaller planet candidate has a radius of \$0.35\textasciitilde\textbackslash pm\textasciitilde 0.04 \textbackslash{} \textbackslash rm R\_\{J\}\$ and orbits a K dwarf (\$K\_\{\textbackslash mathrm\{p\}\}=15.4\$ mag) with a transit depth of 2.9 millimag. The larger planet candidate has a radius of \$0.72\textasciitilde\textbackslash pm\textasciitilde 0.02 \textbackslash{} \textbackslash rm R\_\{J\}\$ and orbits a late G type star (\$K\_\{\textbackslash mathrm\{p\}\}=15.7\$ mag) with a transit depth of 2.2 millimag. The larger planet candidate maybe an unresolved binary or a false alarm. Our catalog includes 44 eclipsing binaries, including ten new detections. Of the eclipsing binaries, one is an M35 member and five are NGC 2158 members. Our catalog contains a total of 1,097 non-transiting variable stars, including a field \$\textbackslash delta\$ Cepheid exhibiting double mode pulsations, 561 rotational variables, and 251 pulsating variables (primarily \$\textbackslash gamma\$ Doradus and \$\textbackslash delta\$ Scuti types). The periods of our catalog sources range between 43 minutes to 24 days. The known ages of our reported cluster variables will facilitate investigations of a variety of stellar evolutionary processes.},
  archivePrefix = {arXiv},
  eprint = {1911.00832},
  eprinttype = {arxiv},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Soares-Furtado et al_2019_A Catalog of Periodic Variables in Open Clusters M35 and NGC 2158.pdf;/home/jdru4706/Zotero/storage/PH224EWL/1911.html},
  journal = {arXiv:1911.00832 [astro-ph]},
  keywords = {Astrophysics - Solar and Stellar Astrophysics},
  note = {Comment: 34 pages, 24 figures},
  primaryClass = {astro-ph}
}

@article{soderblom_ages_2010,
