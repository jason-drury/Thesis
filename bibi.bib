  author = {Silva Aguirre, V. and Chaplin, W. J. and Ballot, J. and Basu, S. and Bedding, T. R. and Serenelli, A. M. and Verner, G. A. and Miglio, A. and Monteiro, M. J. P. F. G. and Weiss, A. and Appourchaux, T. and Bonanno, A. and Broomhall, A. M. and Bruntt, H. and Campante, T. L. and Casagrande, L. and Corsaro, E. and Elsworth, Y. and Garc{\'i}a, R. A. and Gaulme, P. and Handberg, R. and Hekker, S. and Huber, D. and Karoff, C. and Mathur, S. and Mosser, B. and Salabert, D. and Sch{\"o}nrich, R. and Sousa, S. G. and Stello, D. and White, T. R. and {Christensen-Dalsgaard}, J. and Gilliland, R. L. and Kawaler, S. D. and Kjeldsen, H. and Houdek, G. and Metcalfe, T. S. and {Molenda-{\.Z}akowicz}, J. and Thompson, M. J. and Caldwell, D. A. and Christiansen, J. L. and Wohler, B.},
