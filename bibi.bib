}

@article{karaali_absolute-magnitude_2012,
  title = {Absolute-{{Magnitude Calibration}} for {{Red Giants Based}} on {{Colour}}\textendash{{Magnitude Diagrams}} of {{Galactic Clusters}}: {{I}}. {{Calibration}} in {{V}} and {{B}}\textendash{{V}}},
  volume = {29},
  issn = {1448-6083, 1323-3580},
  shorttitle = {Absolute-{{Magnitude Calibration}} for {{Red Giants Based}} on {{Colour}}\textendash{{Magnitude Diagrams}} of {{Galactic Clusters}}},
  abstract = {We present an absolute-magnitude calibration for red giants using the colour\textendash{}magnitude diagrams of six Galactic clusters with different metallicities: M92, M13, M5, 47 Tuc, M67 and NGC 6791. The combination of the absolute magnitude offset from the fiducial of giant sequence of the cluster M5 with the corresponding metallicity offset provides a calibration estimation for the absolute magnitude of red giants for a given (B \textendash{} V)0 colour. The calibration is defined in the colour interval 0.75 {$\leq$} (B \textendash{} V)0 {$\leq$} 1.50 mag and it covers the metal licity interval -2.15 {$>$} [Fe/H]{$\leq$}+0.37 dex. 91\% of the absolute magnitude residuals obtained by the application of the procedure to another set of Galactic clusters lie in the interval -0.40 {$<$} {$\Delta$}M{$\leq$}+0.40 mag. The mean and the standard deviation of the residuals are 0.05 and 0.19 mag, respectively. We fitted the absolute magnitude also to metallicity and age for a limited sub-sample of (B \textendash{} V)0 colour, just to test the effect of age in absolute-magnitude calibration. Comparison of the mean and the standard deviation of the residuals evaluated by this procedure with the corresponding ones provided by the procedure where the absolute magnitude fitted to a third degree polynomial of metallicity show that the age parameter may be omitted in absolute magnitude estimation of red giants. The derived relations are applicable to stars older than 4 Gyr, the age of the youngest calibrating cluster.},
  language = {en},
  number = {4},
  journal = {Publications of the Astronomical Society of Australia},
  doi = {10.1071/AS12003},
  author = {Karaali, S. and Bilir, S. and G{\"o}k{\c c}e, E. Yaz},
  year = {2012/ed},
  keywords = {NGC 6791,globular clusters: individual (M92,M13,M5,stars: distances,47 Tuc,open clusters: individual M67},
  pages = {509-522},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/RAYHQBUD/Karaali et al. - 2012 - Absolute-Magnitude Calibration for Red Giants Base.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/K9L6DTCJ/446314DF222013567F851940B46E6A7F.html}
