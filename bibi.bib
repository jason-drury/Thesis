  author = {Lund, Mikkel N. and Aguirre, V{\'i}ctor Silva and Davies, Guy R. and Chaplin, William J. and {Christensen-Dalsgaard}, J{\o}rgen and Houdek, G{\"u}nter and White, Timothy R. and Bedding, Timothy R. and Ball, Warrick H. and Huber, Daniel and Antia, H. M. and Lebreton, Yveline and Latham, David W. and Handberg, Rasmus and Verma, Kuldeep and Basu, Sarbani and Casagrande, Luca and Justesen, Anders B. and Kjeldsen, Hans and Mosumgaard, Jakob R.},
