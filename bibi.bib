  year = {1992},
  volume = {34},
  pages = {193--202},
  abstract = {We are developing a dedicated, semiautomatic system with which we will conduct a definitive search for massive objects (such as brown dwarfs and Jupiters) which plausibly might comprise the dark matter in the halo of the Milky Way galaxy. This search will be conducted by making photometric measurements on three to ten million Magellanic Cloud (or Galactic Bulge) stars, each night for four years, in order to detect the occasional amplification of these stars by the gravitational microlens effect. Simulations have shown that this effect can be used to survey the mass range 10 exp -7 to 100 solar masses, with sufficient sensitivity to detect ten or more convincing amplification events if objects in this mass range make up the halo dark matter. This means that (i) if these objects make up the halo dark matter, this experiment will detect microlens effects; (ii) if these objects do not comprise the dark matter, this experiment will establish that fact.},
  file = {/home/jdru4706/Zotero/storage/9EAE8XW7/Alcock et al. - 1992 - The search for massive compact halo objects with a.pdf},
  keywords = {Astronomical Photometry,Automatic Control,Brown Dwarf Stars,Galactic Bulge,Galactic Halos,Jupiter (Planet),Magellanic Clouds,Milky Way Galaxy,Robotics,Telescopes}
}

@article{an_comparison_2019,
  title = {Comparison of the {{Asteroseismic Mass Scale}} of {{Red Clump Giants}} with {{Photometric Mass Estimates}}},
  author = {An, Deokkeun and Pinsonneault, Marc H. and Terndrup, Donald M. and Chung, Chul},
