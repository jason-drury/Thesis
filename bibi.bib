@article{gettel_kepler-454_2016,
  title = {{{THE KEPLER}}-454 {{SYSTEM}}: {{A SMALL}}, {{NOT}}-{{ROCKY INNER PLANET}}, {{A JOVIAN WORLD}}, {{AND A DISTANT COMPANION}}},
