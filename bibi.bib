  keywords = {catalogs,methods: statistical,parallaxes,Hertzsprung–Russell and C–M diagrams},
  pages = {145},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/F7NQL5V7/Anderson et al. - 2018 - Improving Gaia Parallax Precision with a Data-driv.pdf}
}

@article{villanova_ngc_2018-4,
