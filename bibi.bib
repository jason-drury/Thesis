  author = {Salabert, D. and Garc{\'i}a, R. A. and Beck, P. G. and Egeland, R. and Pall{\'e}, P. L. and Mathur, S. and Metcalfe, T. S. and {do Nascimento}, J.-D. and Ceillier, T. and Andersen, M. F. and Trivi{\~n}o Hage, A.},
  month = dec,
