@article{huber_revised_2014,
  title = {{{REVISED STELLAR PROPERTIES OF}} {{{\emph{KEPLER}}}} {{TARGETS FOR THE QUARTER}} 1-16 {{TRANSIT DETECTION RUN}}},
  volume = {211},
  issn = {0067-0049, 1538-4365},
  abstract = {We present revised properties for 196,468 stars observed by the NASA Kepler mission and used in the analysis of Quarter 1\textendash{}16 (Q1\textendash{}Q16) data to detect and characterize transiting planets. The catalog is based on a compilation of literature values for atmospheric properties (temperature, surface gravity, and metallicity) derived from different observational techniques (photometry, spectroscopy, asteroseismology, and exoplanet transits), which were then homogeneously fitted to a grid of Dartmouth stellar isochrones. We use broadband photometry and asteroseismology to characterize 11,532 Kepler targets which were previously unclassified in the Kepler Input Catalog (KIC). We report the detection of oscillations in 2762 of these targets, classifying them as giant stars and increasing the number of known oscillating giant stars observed by Kepler by {$\sim$}20\% to a total of {$\sim$}15,500 stars. Typical uncertainties in derived radii and masses are {$\sim$}40\% and {$\sim$}20\%, respectively, for stars with photometric constraints only, and 5\%\textendash{}15\% and {$\sim$}10\% for stars based on spectroscopy and/or asteroseismology, although these uncertainties vary strongly with spectral type and luminosity class. A comparison with the Q1\textendash{}Q12 catalog shows a systematic decrease in radii of M dwarfs, while radii for K dwarfs decrease or increase depending on the Q1\textendash{}Q12 provenance (KIC or Yonsei\textendash{}Yale isochrones). Radii of F\textendash{}G dwarfs are on average unchanged, with the exception of newly identified giants. The Q1\textendash{}Q16 star properties catalog is a first step toward an improved characterization of all Kepler targets to support planet-occurrence studies.},
  language = {en},
  number = {1},
  journal = {The Astrophysical Journal Supplement Series},
  doi = {10.1088/0067-0049/211/1/2},
  author = {Huber, Daniel and Aguirre, Victor Silva and Matthews, Jaymie M. and Pinsonneault, Marc H. and Gaidos, Eric and Garc{\'i}a, Rafael A. and Hekker, Saskia and Mathur, Savita and Mosser, Benoit and Torres, Guillermo and Bastien, Fabienne A. and Basu, Sarbani and Bedding, Timothy R. and Chaplin, William J. and Demory, Brice-Olivier and Fleming, Scott W. and Guo, Zhao and Mann, Andrew W. and Rowe, Jason F. and Serenelli, Aldo M. and Smith, Myron A. and Stello, Dennis},
  month = feb,
  year = {2014},
  pages = {2},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Huber et al. - 2014 - REVISED STELLAR PROPERTIES OF iKEPLERi TARGET.pdf}
}

