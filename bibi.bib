  author = {Derekas, A. and Szab{\'o}, Gy. M. and Berdnikov, L. and Szab{\'o}, R. and Smolec, R. and Kiss, L. L. and Szabados, L. and Chadid, M. and Evans, N. R. and Kinemuchi, K. and Nemec, J. M. and Seader, S. E. and Smith, J. C. and Tenenbaum, P.},
  month = sep,
  year = {2012},
  pages = {1312-1319},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Derekas et al. - 2012 - Period and light-curve fluctuations of the Kepler .pdf}
}

