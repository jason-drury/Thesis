  shorttitle = {The Masses of Retired {{A}} Stars with Asteroseismology},
  author = {North, Thomas S. H. and Campante, Tiago L. and Miglio, Andxsrea and Davies, Guy R. and Grunblatt, Samuel K. and Huber, Daniel and Kuszlewicz, James S. and Lund, Mikkel N. and Cooke, Benjamin F. and Chaplin, William J.},
  year = {2017},
