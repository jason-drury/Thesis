  keywords = {Astrophysics - Solar and Stellar Astrophysics},
  pages = {L8},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/KBGBAQC3/Slumstrup et al. - 2017 - The [YMg] clock works for evolved solar metallici.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/EFGMLR4R/1707.html},
