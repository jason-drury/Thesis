@article{corsaro_metallicity_2017-2,
  archivePrefix = {arXiv},
  eprinttype = {arxiv},
  eprint = {1707.07474},
  primaryClass = {astro-ph},
  title = {Metallicity Effect on Stellar Granulation Detected from Oscillating Red Giants in Open Clusters},
  abstract = {The effect of metallicity on the granulation activity in stars is still poorly understood. Available spectroscopic parameters from the updated APOGEE-\textbackslash{}textit\{Kepler\} catalog, coupled with high-precision photometric observations from NASA's \textbackslash{}textit\{Kepler\} mission spanning more than four years of observation, make oscillating red giant stars in open clusters crucial testbeds. We determine the role of metallicity on the stellar granulation activity by discriminating its effect from that of different stellar properties such as surface gravity, mass, and temperature. We analyze 60 known red giant stars belonging to the open clusters NGC 6791, NGC 6819, and NGC 6811, spanning a metallicity range from [Fe/H] \$\textbackslash{}simeq -0.09\$ to \$0.32\$. The parameters describing the granulation activity of these stars and their \$\textbackslash{}nu\_\textbackslash{}mathrm\{max\}\$, are studied by considering the different masses, metallicities, and stellar evolutionary stages. We derive new scaling relations for the granulation activity, re-calibrate existing ones, and identify the best scaling relations from the available set of observations. We adopted the Bayesian code DIAMONDS for the analysis of the background signal in the Fourier spectra of the stars. We performed a Bayesian parameter estimation and model comparison to test the different model hypotheses proposed in this work and in the literature. Metallicity causes a statistically significant change in the amplitude of the granulation activity, with a dependency stronger than that induced by both stellar mass and surface gravity. We also find that the metallicity has a significant impact on the corresponding time scales of the phenomenon. The effect of metallicity on the time scale is stronger than that of mass. A higher metallicity increases the amplitude of granulation and meso-granulation signals and slows down their characteristic time scales toward longer periods.},
  journal = {arXiv:1707.07474 [astro-ph]},
  doi = {10.1051/0004-6361/201731094},
  author = {Corsaro, E. and Mathur, S. and Garc{\'i}a, R. A. and Gaulme, P. and Pinsonneault, M. and Stassun, K. and Stello, D. and Tayar, J. and Trampedach, R. and Jiang, C. and Nitschelm, C. and Salabert, D.},
  month = jul,
  year = {2017},
  keywords = {Astrophysics - Solar and Stellar Astrophysics},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/F694USTN/Corsaro et al. - 2017 - Metallicity effect on stellar granulation detected.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/ZMQ25KT8/1707.html},
  annote = {Comment: 20 pages, 11 figures, 9 tables. Accepted for publication in A\&A}
}

@article{linden_timing_2017-1,
  archivePrefix = {arXiv},
  eprinttype = {arxiv},
  eprint = {1704.07305},
  primaryClass = {astro-ph},
  title = {Timing the {{Evolution}} of the {{Galactic Disk}} with {{NGC}} 6791: {{An Open Cluster}} with {{Peculiar High}}-\$\textbackslash{}alpha\$ {{Chemistry}} as Seen by {{APOGEE}}},
  shorttitle = {Timing the {{Evolution}} of the {{Galactic Disk}} with {{NGC}} 6791},
  abstract = {We utilize elemental-abundance information for Galactic red giant stars in five open clusters (NGC 7789, NGC 6819, M67, NGC 188, and NGC 6791) from the Apache Point Observatory Galactic Evolution Experiment (APOGEE) DR13 dataset to age-date the chemical evolution of the high- and low-\$\textbackslash{}alpha\$ element sequences of the Milky Way. Key to this time-stamping is the cluster NGC 6791, whose stellar members have mean abundances that place it in the high-\$\textbackslash{}alpha\$, high-[Fe/H] region of the [\$\textbackslash{}alpha\$/Fe]-[Fe/H] plane. Based on the cluster's age (\$\textbackslash{}sim 8\$ Gyr), Galactocentric radius, and height above the Galactic plane, as well as comparable chemistry reported for APOGEE stars in Baade's Window, we suggest that the two most likely origins for NGC 6791 are as an original part of the thick-disk, or as a former member of the Galactic bulge. Moreover, because NGC 6791 lies at the \textbackslash{}textit\{high metallicity end\} ([Fe/H] \$\textbackslash{}sim 0.4\$) of the high-\$\textbackslash{}alpha\$ sequence, the age of NGC 6791 places a limit on the \textbackslash{}textit\{youngest age\} of stars in the high-metallicity, high-\$\textbackslash{}alpha\$ sequence for the cluster's parent population (i.e., either the bulge or the disk). In a similar way, we can also use the age and chemistry of NGC 188 to set a limit of \$\textbackslash{}sim 7\$ Gyr on the \textbackslash{}textit\{oldest age\} of the low-\$\textbackslash{}alpha\$ sequence of the Milky Way. Therefore, NGC 6791 and NGC 188 are potentially a pair of star clusters that bracket both the timing and the duration of an important transition point in the chemical history of the Milky Way.},
  journal = {arXiv:1704.07305 [astro-ph]},
  doi = {10.3847/1538-4357/aa6f17},
  author = {Linden, Sean T. and Pryal, Matthew and Hayes, Christian R. and Troup, Nicholas W. and Majewski, Steven R. and Andrews, Brett H. and Beers, Timothy C. and Carrera, Ricardo and Cunha, Katia and {Fern{\'a}ndez-Trincado}, J. G. and Frinchaboy, Peter and Geisler, Doug and Lane, Richard R. and Nitschelm, Christian and Pan, Kaike and Prieto, Carlos Allende and {Roman-Lopes}, Alexandre and Smith, Verne V. and Sobeck, Jennifer and Tang, Baitian and Villanova, Sandro and Zasowski, Gail},
  month = apr,
  year = {2017},
  keywords = {Astrophysics - Astrophysics of Galaxies},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/PK5NPUXD/Linden et al. - 2017 - Timing the Evolution of the Galactic Disk with NGC.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/6KS3PLYH/1704.html},
  annote = {Comment: 15 pages, 6 figures, 2 tables; Accepted for Publication in ApJ}
}

@article{rodrigues_determining_2017,
  archivePrefix = {arXiv},
  eprinttype = {arxiv},
  eprint = {1701.04791},
  title = {Determining Stellar Parameters of Asteroseismic Targets: Going beyond the Use of Scaling Relations},
