@article{giles_systematic_2019-1,
  archivePrefix = {arXiv},
  eprinttype = {arxiv},
  eprint = {1812.07156},
  title = {Systematic {{Serendipity}}: {{A Test}} of {{Unsupervised Machine Learning}} as a {{Method}} for {{Anomaly Detection}}},
  volume = {484},
  issn = {0035-8711, 1365-2966},
  shorttitle = {Systematic {{Serendipity}}},
  abstract = {Advances in astronomy are often driven by serendipitous discoveries. As survey astronomy continues to grow, the size and complexity of astronomical databases will increase, and the ability of astronomers to manually scour data and make such discoveries decreases. In this work, we introduce a machine learning-based method to identify anomalies in large datasets to facilitate such discoveries, and apply this method to long cadence lightcurves from NASA's Kepler Mission. Our method clusters data based on density, identifying anomalies as data that lie outside of dense regions. This work serves as a proof-of-concept case study and we test our method on four quarters of the Kepler long cadence lightcurves. We use Kepler's most notorious anomaly, Boyajian's Star (KIC 8462852), as a rare `ground truth' for testing outlier identification to verify that objects of genuine scientific interest are included among the identified anomalies. We evaluate the method's ability to identify known anomalies by identifying unusual behavior in Boyajian's Star, we report the full list of identified anomalies for these quarters, and present a sample subset of identified outliers that includes unusual phenomena, objects that are rare in the Kepler field, and data artifacts. By identifying {$<$}4\% of each quarter as outlying data, we demonstrate that this anomaly detection method can create a more targeted approach in searching for rare and novel phenomena.},
  number = {1},
  journal = {Monthly Notices of the Royal Astronomical Society},
  doi = {10.1093/mnras/sty3461},
  author = {Giles, Daniel and Walkowicz, Lucianne},
  month = mar,
  year = {2019},
  keywords = {Astrophysics - Solar and Stellar Astrophysics,Astrophysics - Instrumentation and Methods for Astrophysics},
  pages = {834-849},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/LQJM5INM/Giles and Walkowicz - 2019 - Systematic Serendipity A Test of Unsupervised Mac.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/5VE2FAA8/1812.html},
  annote = {Comment: 21 pages, 10 figures, Submitted to MNRAS}
}

@article{ilin_flares_2019-1,
  archivePrefix = {arXiv},
  eprinttype = {arxiv},
  eprint = {1812.06725},
  title = {Flares in {{Open Clusters}} with {{K2}}. {{I}}. {{M45}} ({{Pleiades}}), {{M44}} ({{Praesepe}}) and {{M67}}},
  volume = {622},
  issn = {0004-6361, 1432-0746},
  abstract = {The presence and strength of a stellar magnetic field and activity is rooted in a star's fundamental parameters such as mass and age. Can flares serve as an accurate stellar "clock"? To explore if we can quantify an activity-age relation in the form of a flaring-age relation, we measured trends in the flaring rates and energies for stars with different masses and ages. We investigated the time-domain photometry provided by Kepler's follow-up mission K2 and searched for flares in three solar metallicity open clusters with well-known ages, M45 (0.125 Gyr), M44 (0.63 Gyr), and M67 (4.3 Gyr). We updated and employed the automated flare finding and analysis pipeline Appaloosa, originally designed for Kepler. We introduced a synthetic flare injection and recovery subroutine to ascribe detection and energy recovery rates for flares in a broad energy range for each light curve. We collected a sample of 1 761 stars, mostly late-K to mid-M dwarfs and found 751 flare candidates with energies ranging from \$4\textbackslash{}cdot10\^\{32\}\$ erg to \$6\textbackslash{}cdot10\^\{34\}\$ erg, of which 596 belong to M45, 155 to M44, and none to M67. We find that flaring activity depends both on \$T\_\textbackslash{}mathrm\{eff\}\$, and age. But all flare frequency distributions have similar slopes with \$\textbackslash{}alpha \textbackslash{}approx2.0-2.4\$, supporting a universal flare generation process. We discuss implications for the physical conditions under which flares occur, and how the sample's metallicity and multiplicity affect our results.},
  journal = {Astronomy \& Astrophysics},
  doi = {10.1051/0004-6361/201834400},
  author = {Ilin, Ekaterina and Schmidt, Sarah J. and Davenport, James R. A. and Strassmeier, Klaus G.},
  month = feb,
  year = {2019},
  keywords = {Astrophysics - Solar and Stellar Astrophysics},
