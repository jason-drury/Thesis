@article{carraro_ub_2013,
  title = {{{UB CCD Photometry}} of the {{Old}}, {{Metal}}-{{Rich}}, {{Open Clusters NGC}} 6791, {{NGC}} 6819, and {{NGC}} 7142},
  author = {Carraro, G. and Buzzoni, A. and Bertone, E. and Buson, L.},
