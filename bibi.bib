@article{li_period_2018-1,
  title = {Period Spacings of {$\gamma$} {{Doradus}} Pulsators in the {{Kepler}} field: {{Rossby}} and Gravity Modes of 70 Stars},
  abstract = {Rossby modes are an oscillation mode in the rotating fluid, whose restoring force is the Coriolis force. They give a new method to understand the rotation of stars, which presently hinders asteroseismic modelling, but is still poorly-observed. We report 70 {$\gamma$} Doradus stars containing clear period spacing patterns of both gravity and Rossby modes. The period spacings of both show a linear relation with the pulsation period but the slope is negative for the gravity modes and positive for the Rossby modes. Most Rossby modes have k = -2, m = -1 and only one series of k = -1, m = -1 modes is seen. For each pattern, the mean pulsation period, the mean period spacing, and the slope are measured. We find the slope correlates with the mean period for Rossby patterns. The traditional approximation of rotation is used to fit the near-core rotation rate assuming the star rotates rigidly. We report the near-core rotation rates, the asymptotic spacings, and the radial orders of excited modes of these 70 main-sequence stars. The near-core rotation rates are between 0.6 d-1 and 2.3 d-1. The radial orders of excited modes show different distributions for the dipole and quadrupole gravity modes, and the Rossby modes.},
  language = {en},
  author = {Li, Gang and Reeth, Timothy Van and Bedding, Timothy R and Murphy, Simon J and Antoci, Victoria},
  year = {2018},
  pages = {16},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Li et al. - 2018 - Period spacings of γ Doradus pulsators in the Kepl.pdf}
}

