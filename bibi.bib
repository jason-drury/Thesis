@article{spina_chemical_2018,
  archivePrefix = {arXiv},
  eprinttype = {arxiv},
  eprint = {1807.00941},
  primaryClass = {astro-ph},
  title = {Chemical Inhomogeneities in the {{Pleiades}}: Signatures of Rocky-Forming Material in Stellar Atmospheres},
  shorttitle = {Chemical Inhomogeneities in the {{Pleiades}}},
  abstract = {The aim of Galactic archaeology is to recover the history of our Galaxy through the information encoded in stars. An unprobed assumption of this field is that the chemical composition of a star is an immutable marker of the gas from which it formed. It is vital to test this assumption on open clusters, group of stars formed from the same gas. Previous investigations have shown that unevolved stars in clusters are chemically homogeneous within the typical uncertainties of these analysis, i.e. 15\$\textbackslash\%\$ of the elemental abundances. Our strictly differential analysis on five members of the Pleiades allows us to reach precisions of 5\$\textbackslash\%\$ for most elements and to unveil chemical anomalies within the cluster that could be explained by planet engulfment events. These results reveal that the evolution of planetary systems may alter the chemical composition of stars, challenging our capability of tagging them to their native environments, and also paving the way for the study of planetary architectures and their evolution, through the chemical pattern of their host stars.},
  journal = {arXiv:1807.00941 [astro-ph]},
  author = {Spina, Lorenzo and Mel{\'e}ndez, Jorge and Casey, Andrew R. and Karakas, Amanda I. and {Tucci-Maia}, Marcelo},
  month = jul,
  year = {2018},
  keywords = {Astrophysics - Solar and Stellar Astrophysics},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/5G7P99TE/Spina et al. - 2018 - Chemical inhomogeneities in the Pleiades signatur.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/6SWW3UEX/1807.html},
  annote = {Comment: 10 pages, 2 figures. Accepted for publication in ApJ}
}

@article{yu_asteroseismology_2018,
  title = {Asteroseismology of 16,000 {{Kepler Red Giants}}: {{Global Oscillation Parameters}}, {{Masses}}, and {{Radii}}},
  volume = {236},
  issn = {0067-0049},
  shorttitle = {Asteroseismology of 16,000 {{Kepler Red Giants}}},
  abstract = {The Kepler mission has provided exquisite data to perform an ensemble asteroseismic analysis on evolved stars. In this work we systematically characterize solar-like oscillations and granulation for 16,094 oscillating red giants, using end-of-mission long-cadence data. We produced a homogeneous catalog of the frequency of maximum power (typical uncertainty \#\#IMG\#\# [http://ej.iop.org/images/0067-0049/236/2/42/apjsaaaf74ieqn1.gif] \$\textbackslash{}sigma \_\textbackslash{}nu \_\textbackslash{}max =1.6 \% \$ ), the mean large frequency separation ( \#\#IMG\#\# [http://ej.iop.org/images/0067-0049/236/2/42/apjsaaaf74ieqn2.gif] \$\textbackslash{}sigma \_\textbackslash{}rm\textbackslash{}Delta \textbackslash{}nu =0.6 \% \$ ), oscillation amplitude ( \#\#IMG\#\# [http://ej.iop.org/images/0067-0049/236/2/42/apjsaaaf74ieqn3.gif] \$\textbackslash{}sigma \_\textbackslash{}rmA=4.7 \% \$ ), granulation power ( \#\#IMG\#\# [http://ej.iop.org/images/0067-0049/236/2/42/apjsaaaf74ieqn4.gif] \$\textbackslash{}sigma \_\textbackslash{}mathrmgran=8.6 \% \$ ), power excess width ( \#\#IMG\#\# [http://ej.iop.org/images/0067-0049/236/2/42/apjsaaaf74ieqn5.gif] \$\textbackslash{}sigma \_\textbackslash{}mathrmwidth=8.8 \% \$ ), seismically derived stellar mass ( \#\#IMG\#\# [http://ej.iop.org/images/0067-0049/236/2/42/apjsaaaf74ieqn6.gif] \$\textbackslash{}sigma \_\textbackslash{}rmM=7.8 \% \$ ), radius ( \#\#IMG\#\# [http://ej.iop.org/images/0067-0049/236/2/42/apjsaaaf74ieqn7.gif] \$\textbackslash{}sigma \_\textbackslash{}rmR=2.9 \% \$ ), and thus surface gravity ( \#\#IMG\#\# [http://ej.iop.org/images/0067-0049/236/2/42/apjsaaaf74ieqn8.gif] \$\textbackslash{}sigma \_\textbackslash{}mathrmlogg=0.01\$ dex). Thanks to the large red giant sample, we confirm that red-giant-branch (RGB) and helium-core-burning (HeB) stars collectively differ in the distribution of oscillation amplitude, granulation power, and width of power excess, which is mainly due to the mass difference. The distribution of oscillation amplitudes shows an extremely sharp upper edge at fixed \#\#IMG\#\# [http://ej.iop.org/images/0067-0049/236/2/42/apjsaaaf74ieqn9.gif] \$\textbackslash{}nu \_\textbackslash{}max \$ , which might hold clues for understanding the excitation and damping mechanisms of the oscillation modes. We find that both oscillation amplitude and granulation power depend on metallicity, causing a spread of 15\% in oscillation amplitudes and a spread of 25\% in granulation power from [Fe/H] = -0.7 to 0.5 dex. Our asteroseismic stellar properties can be used as reliable distance indicators and age proxies for mapping and dating galactic stellar populations observed by Kepler . They will also provide an excellent opportunity to test asteroseismology using Gaia parallaxes, and lift degeneracies in deriving atmospheric parameters in large spectroscopic surveys such as APOGEE and LAMOST.},
  language = {en},
  number = {2},
  journal = {ApJS},
  doi = {10.3847/1538-4365/aaaf74},
  author = {Yu, Jie and Huber, Daniel and Bedding, Timothy R. and Stello, Dennis and Hon, Marc and Murphy, Simon J. and Khanna, Shourya},
  year = {2018},
  keywords = {stars: fundamental parameters,stars: oscillations,catalogs,techniques: photometric},
  pages = {42},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/WPYJEDAV/Yu et al. - 2018 - Asteroseismology of 16,000 Kepler Red Giants Glob.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/XWRUYKXJ/Yu et al. - 2018 - Asteroseismology of 16,000 Kepler Red Giants Glob.pdf}
}

@article{hon_deep_2018,
  title = {Deep Learning Classification in Asteroseismology Using an Improved Neural Network: Results on 15 000 {{Kepler}} Red Giants and Applications to {{K2}} and {{TESS}} Data},
  abstract = {Deep learning in the form of 1D convolutional neural networks have previously been shown to be capable of efficiently classifying the evolutionary state of oscillating red giants into red giant branch stars and helium-core burning stars by recognizing visual features in their asteroseismic frequency spectra. We elaborate further on the deep learning method by developing an improved convolutional neural network classifier. To make our method useful for current and future space missions such as K2, TESS, and PLATO, we train classifiers that are able to classify the evolutionary states of lower frequency resolution spectra expected from these missions. Additionally, we provide new classifications for 8633 Kepler red giants, out of which 426 have previously not been classified using asteroseismology. This brings the total to 14983 Kepler red giants classified with our new neural network. We also verify that our classifiers are remarkably robust to suboptimal data, including low signal-to-noise and incorrect training truth labels.},
