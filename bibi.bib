}

@article{morrison_globular_2016,
  title = {Globular and {{Open Clusters Observed}} by {{SDSS}}/{{SEGUE}}: {{The Giant Stars}}},
  volume = {151},
  issn = {1538-3881},
  shorttitle = {Globular and {{Open Clusters Observed}} by {{SDSS}}/{{SEGUE}}},
  abstract = {We present griz observations for the clusters M92, M13 and NGC 6791 and gr photometry for M71, Be 29 and NGC 7789. In addition we present new membership identifications for all these clusters, which have been observed spectroscopically as calibrators for the Sloan Digital Sky Survey (SDSS)/SEGUE survey; this paper focuses in particular on the red giant branch stars in the clusters. In a number of cases, these giants were too bright to be observed in the normal SDSS survey operations, and we describe the procedure used to obtain spectra for these stars. For M71, we also present a new variable reddening map and a new fiducial for the gr giant branch. For NGC 7789, we derived a transformation from T eff to g \textendash{} r for giants of near solar abundance, using IRFM T eff measures of stars with good ugriz and 2MASS photometry and SEGUE spectra. The result of our analysis is a robust list of known cluster members with correctly dereddened and (if needed) transformed gr photometry for crucial calibration efforts for SDSS and SEGUE.},
  language = {en},
  number = {1},
  journal = {AJ},
  doi = {10.3847/0004-6256/151/1/7},
  author = {Morrison, Heather L. and Ma, Zhibo and Clem, James L. and An, Deokkeun and Connor, Thomas and {Schechtman-Rook}, Andrew and {Luca Casagrande} and Rockosi, Constance and Yanny, Brian and Harding, Paul and Beers, Timothy C. and Johnson, Jennifer A. and Schneider, Donald P.},
  year = {2016},
  keywords = {NGC 6791,Membership},
  pages = {7},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/P5WGLYL2/Morrison et al. - 2016 - Globular and Open Clusters Observed by SDSSSEGUE.pdf;/home/jdru4706/Dropbox_USyd/Papers/storage/WXQ6K3AW/Morrison et al. - 2015 - GLOBULAR AND OPEN CLUSTERS OBSERVED BY SDSSSEGUE.pdf}
