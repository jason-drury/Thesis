  month = mar,
  year = {2019},
  keywords = {open clusters and associations: general,stars: abundances},
  pages = {A80},
  file = {/home/jdru4706/Dropbox_USyd/Papers/storage/A7LQEG5Q/Carrera et al. - 2019 - Open clusters in APOGEE and GALAH. Combining Gaia .pdf}
