@article{kurtz_unifying_2015,
  title = {A Unifying Explanation of Complex Frequency Spectra of {$\gamma$} {{Dor}}, {{SPB}} and {{Be}} Stars: Combination Frequencies and Highly Non-Sinusoidal Light Curves},
  shorttitle = {A Unifying Explanation of Complex Frequency Spectra of {$\gamma$} {{Dor}}, {{SPB}} and {{Be}} Stars},
  author = {Kurtz, Donald W. and Shibahashi, Hiromoto and Murphy, Simon J. and Bedding, Timothy R. and Bowman, Dominic M.},
  year = {2015},
  month = jul,
  volume = {450},
