  month = may,
  year = {2018},
  pages = {1931-1941},
  file = {/home/jdru4706/Dropbox_USyd/Papers/kepler-papers/Sahlholdt et al. - 2018 - Testing asteroseismic radii of dwarfs and subgiant.pdf}
}

