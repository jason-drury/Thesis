@article{peterson_kepler_2012,
  title = {Kepler and {{Red Giants}} in the {{Old Open Cluster NGC}} 6791},
  volume = {220},
  abstract = {This work illustrates several ways in which Kepler observations obtained through our Cycle 2 and 3 GO programs are better characterizing the red giant population in NGC 6791. This metal-rich cluster is critically important both for its stellar properties and as a nearby template for old stellar systems within and beyond our Galaxy, such as the ``red-and-dead'' elliptical galaxies. It is a massive, old, nearby cluster with a metallicity three times solar. Its binary fraction is high, about 50\% among stars on or near the main sequence. We present a progress report on the detection of detached eclipsing binaries among giants, the primary goal of this program. We illustrate the discrimination of field non-members from true members of similar magnitude using Kepler light curves, and the results so far from our survey of potential members at large distances from the cluster center. Among the brighter stars in the sample, we relate the Kepler astroseismological signatures to spectroscopic results obtained for the same stars.},
  language = {en},
  journal = {American Astronomical Society Meeting Abstracts \#220},
  author = {Peterson, Ruth},
  month = may,
  year = {2012},
  keywords = {NGC 6791,Membership},
  pages = {406.05}
}

@article{zhao_fragile_2012,
