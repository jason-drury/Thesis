  language = {en}
}

@article{cummings_improved_2018,
  title = {Improved {{Main Sequence Turnoff Ages}} of {{Young Open Clusters}}: {{Multicolor UBV Techniques}} \& the {{Challenges}} of {{Rotation}}},
  shorttitle = {Improved {{Main Sequence Turnoff Ages}} of {{Young Open Clusters}}},
  author = {Cummings, Jeffrey D. and Kalirai, Jason S.},
  year = {2018},
  month = sep,
  volume = {156},
  pages = {165},
  issn = {1538-3881},
  doi = {10.3847/1538-3881/aad5df},
  abstract = {Main sequence turnoff ages in young open clusters are complicated by turnoffs that are sparse, have high binarity fractions, can be affected by differential reddening, and typically include a number of peculiar stars. Furthermore, stellar rotation can have a significant effect on a star's photometry and evolutionary timescale. In this paper we analyze in 12 nearby open clusters, ranging from ages of 50 Myr to 350 Myr, how broadband UBV color-color relations can be used to identify turnoff stars that are Be stars, blue stragglers, certain types of binaries, or those affected by differential reddening. This UBV color-color analysis also directly measures a cluster's E(B-V) and estimates its [Fe/H]. The turnoff stars unaffected by these peculiarities create a narrower and more clearly defined cluster turnoff. Using four common isochronal models, two of which consider rotation, we fit cluster parameters using these selected turnoff stars and the main sequence. Comparisons of the photometrically fit cluster distances to those based on Gaia DR2 parallaxes find that they are consistent for all clusters. For older ({$>$}100 Myr) clusters, like the Pleiades and the Hyades, comparisons to ages based on the lithium depletion boundary method finds that these cleaned turnoff ages agree to within \textasciitilde 10\% for all four isochronal models. For younger clusters, however, only the Geneva models that consider rotation fit turnoff ages consistent with lithium-based ages, while the ages based on non-rotating isochrones quickly diverge to become 30\% to 80\% younger. This illustrates the importance of rotation for deriving ages in the youngest ({$<$}100 Myr) clusters.},
  archivePrefix = {arXiv},
  eprint = {1807.10772},
  eprinttype = {arxiv},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Cummings_Kalirai_2018_Improved Main Sequence Turnoff Ages of Young Open Clusters.pdf;/home/jdru4706/Zotero/storage/7X7EVH7P/1807.html},
  journal = {The Astronomical Journal},
  keywords = {Astrophysics - Solar and Stellar Astrophysics},
  note = {Comment: Accepted for publication by AJ, 18 pages, 12 figures},
  number = {4}
}

@article{cummings_search_2014,
  title = {Search for {{Binaries}} in the {{Extended Horizontal Branch}} of {{NGC}} 6791},
  author = {Cummings, Jeffrey and Kalirai, Jason},
  year = {2014},
  month = aug,
  pages = {308},
  abstract = {Extended horizontal branch (EHB) stars are common but poorly understood stars observed in the field (subdwarf B/O; sdB/sdO), in globular clusters, and in the metal-rich open cluster NGC 6791. These stars are very hot with extremely thin hydrogen-rich envelopes, which requires that they have undergone significant mass loss beyond that explained by models. A common theory is that their envelopes have been stripped by binary companions. While a majority of the field sdB stars are found to be short-period binaries, spectroscopically observed EHB stars in globular cluster are predominantly single stars. Does this suggest that their mechanisms of formation are distinct in these differing environments? We propose to test this by spectroscopically targeting the EHB stars in the open cluster NGC 6791. The nature of the EHB population in this cluster provides two potentially exciting results. If we find that the EHB stars are binaries, it suggests that in the significantly more dense environment of globulars their binarity has been disrupted. Otherwise, if we find they are singly evolved, this suggests that either the multiple populations observed in both environments or the very high metallicity, if not both, plays an important role. Multiple epochs of brief spectroscopic observations of these EHB stars in NGC 6791 will verify their cluster membership, detect if they are short period binaries, determine their v sin i, and be used to fit models for determining their stellar parameters.},
  file = {/home/jdru4706/Zotero/storage/CFGAFVNC/ui.adsabs.harvard.edu.html},
  journal = {NOAO Proposal},
  keywords = {Membership,NGC 6791},
  language = {en}
}

@article{cummings_white_2018,
  title = {The {{White Dwarf Initial}}-{{Final Mass Relation}} for {{Progenitor Stars}} from 0.85 to 7.5 {{M}} {\textsubscript{{$\Sun$}}}},
  author = {Cummings, Jeffrey D. and Kalirai, Jason S. and Tremblay, P.-E. and {Ramirez-Ruiz}, Enrico and Choi, Jieun},
  year = {2018},
  month = oct,
  volume = {866},
  pages = {21},
  doi = {10.3847/1538-4357/aadfd6},
  abstract = {We present the initial-final mass relation (IFMR) based on the self-consistent analysis of Sirius B and 79 white dwarfs from 13 star clusters. We have also acquired additional signal on eight white dwarfs previously analyzed in the NGC 2099 cluster field, four of which are consistent with membership. These re-observed white dwarfs have masses ranging from 0.72 to 0.97 M \textsubscript{{$\Sun$}}, with initial masses from 3.0 to 3.65 M \textsubscript{{$\Sun$}}, where the IFMR has an important change in slope that these new data help to observationally confirm. In total, this directly measured IFMR has small scatter ({$\sigma$} = 0.06 M \textsubscript{{$\Sun$}}) and spans from progenitors of 0.85 to 7.5 M \textsubscript{{$\Sun$}}. Applying two different stellar evolutionary models to infer two different sets of white dwarf progenitor masses shows that, when the same model is also used to derive the cluster ages, the resulting IFMR has weak sensitivity to the adopted model at all but the highest initial masses (\&gt;5.5 M \textsubscript{{$\Sun$}}). The nonlinearity of the IFMR is also clearly observed with moderate slopes at lower masses (0.08 M \textsubscript{final}/M \textsubscript{initial}) and higher masses (0.11 M \textsubscript{final}/M \textsubscript{initial}) that are broken up by a steep slope (0.19 M \textsubscript{final}/M \textsubscript{initial}) between progenitors from 2.85 to 3.6 M \textsubscript{{$\Sun$}}. This IFMR shows total stellar mass loss ranges from 33\% of M \textsubscript{initial} at 0.83 M \textsubscript{{$\Sun$}} to 83\% of M \textsubscript{initial} at 7.5 M \textsubscript{{$\Sun$}}. Testing this total mass loss for dependence on progenitor metallicity, however, finds no detectable sensitivity across the moderate range of -0.15 \&lt; [Fe/H] \&lt; +0.15. Based on observations with the W.M. Keck Observatory, which is operated as a scientific partnership among the California Institute of Technology, the University of California, and NASA, was made possible by the generous financial support of the W.M. Keck Foundation.},
  file = {/home/jdru4706/Dropbox_USyd/Apps/Zotero/Cummings et al_2018_The White Dwarf Initial-Final Mass Relation for Progenitor Stars from 0.pdf;/home/jdru4706/Zotero/storage/CGVQLCXN/abstract.html},
  journal = {The Astrophysical Journal},
  language = {en},
  number = {1}
}

