  doi = {10.1007/s10509-014-2175-5},
  author = {Yontan, T. and Bilir, S. and Bostanc\i, Z. F. and Ak, T. and Karaali, S. and G{\"u}ver, T. and Ak, S. and Duran, S. and Paunzen, E.},
